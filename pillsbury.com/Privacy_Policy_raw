<script>
    $(function () {
        $("#InitNav").click(function () {
            if (!$("#InitNav").hasClass("close-nav") && !$("#InitNav").hasClass("open-nav")) {
                $("#InitNav").addClass("open-nav");
            }
            else {
                if ($("#InitNav").hasClass('close-nav')) {
                    $("#InitNav").removeClass("close-nav").addClass("open-nav");

                } else {
                    $("#InitNav").removeClass("open-nav").addClass("close-nav");
                }
            }
        });
    });

</script>
