    jQuery('document').ready(function () {
        _setupActionBoxes({ loggedIn: false });
    });


    //RegACtion for Action Boxes
    function _setupActionBoxes(options) {
        jQuery(".ab_writeareview").text("Review");
        jQuery(".ab_addtofavorites").text("Save");

        var recipeDetailRegex = /\/recipes\/[^\/]+\/[0-9A-F]{8}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{12}/i;
        var isRecipe = true;
        if (!document.location.href.match(recipeDetailRegex)) {
            isRecipe = false;
        }
        if (!options.loggedIn) {

            //custom set up for save a recipe

            var $addtoFav = $('.ab_addtofavorites');
            if ($addtoFav.length > 0) {
            var returnSaveVar = $addtoFav.get(0).onclick.toString().split("(");

            jQuery(".ab_addtofavorites").removeAttr("click");

            jQuery(".ab_addtofavorites").click(function() {


                var delimitedstr = returnSaveVar[2];
                var str2 = delimitedstr.toString();
                var delimitedstr2 = delimitedstr.split(")");

                options = delimitedstr2[0];

                var obj = jQuery.parseJSON(options);
                if (isRecipe) {
                    obj.RegAction = "SaveRecipe";
                } else {
                    obj.RegAction = "SaveContent";
                }
                // reg.userRegistration.openRegistrationDialog(obj);
                showLogin(obj);
            });
        }
        var $addtoGrocery = $('.ab_addtogrocerylist');
            if ($addtoGrocery.length > 0) {
            var returnSaveGroceryVar = $addtoGrocery.get(0).onclick.toString().split("(");

            jQuery(".ab_addtogrocerylist").removeAttr("click");

            jQuery(".ab_addtogrocerylist").click(function () {


                var delimitedstr = returnSaveGroceryVar[2];
                var str2 = delimitedstr.toString();
                var delimitedstr2 = delimitedstr.split(")");

                options = delimitedstr2[0];

                var obj = jQuery.parseJSON(options);

                obj.RegAction = "GroceryList";
                showLogin(obj);
            });}
            //custom set up for review a recipe
            //var $writeReview = $('.ab_writeareview');
            //var returnReviewVar = $writeReview.get(0).onclick.toString().split("(");


            //jQuery(".ab_writeareview").removeAttr("onclick");

            //jQuery(".ab_writeareview").click(function () {


            //    var delimitedstr = returnReviewVar[2];
            //    var str2 = delimitedstr.toString();
            //    var delimitedstr2 = delimitedstr.split(")");

            //    options = delimitedstr2[0];

            //    var obj = jQuery.parseJSON(options);
            //    obj.RegAction = "RecipeReview";
            //    //reg.userRegistration.openRegistrationDialog(obj);
            //    showLogin(obj);
            //});
        }
        
    }
</script>
