try {
var referrer = dojo.cookie('referrerForAdroitPixel');
if (!referrer && document.referrer) {
var pos = document.referrer.indexOf(document.location.host);
if (pos < 0 || pos > 9) {
dojo.cookie('referrerForAdroitPixel', document.referrer, {path:'/'});
}
}
} catch (e) {
console.error(e);
}
</script>
