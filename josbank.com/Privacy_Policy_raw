try {
marketingPixelsData.googleSmartConfig = marketingPixelsData.googleSmartConfig || {};
if (typeof marketingPixelsData.googleSmartConfig.pagetype == 'undefined' || marketingPixelsData.googleSmartConfig.pagetype == '') {
if (marketingPixelsData.currentPage == 'CartPage') {
marketingPixelsData.googleSmartConfig.pagetype = 'cart';
} else if (marketingPixelsData.currentPage == 'ReceiptPage') {
marketingPixelsData.googleSmartConfig.pagetype = 'purchase';
} else if (marketingPixelsData.currentPage == 'ProductPage') {
marketingPixelsData.googleSmartConfig.pagetype = 'product';
} else if (marketingPixelsData.currentPage == 'HomePage') {
marketingPixelsData.googleSmartConfig.pagetype = 'home';
} else if (marketingPixelsData.currentPage == 'SearchResultPage') {
marketingPixelsData.googleSmartConfig.pagetype = 'searchresults ';
} else if (marketingPixelsData.currentPage == 'CategoryPage') {
marketingPixelsData.googleSmartConfig.pagetype = 'category ';
} else {
marketingPixelsData.googleSmartConfig.pagetype = 'other';
}
}
} catch(e) {
console.error(e);
}
</script>
