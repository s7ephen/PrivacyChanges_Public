try {
var affiliateParam = getParameterByName('cm_mmc');
if (affiliateParam && affiliateParam.lastIndexOf('CJ', 0) == 0) {
dojo.cookie('affiliateReferrer', 'CJ', {path:'/', expires: 30});
} else if (affiliateParam && affiliateParam.lastIndexOf('Linkshare', 0) == 0) {
dojo.cookie('affiliateReferrer', 'LS', {path:'/', expires: 30});
}
} catch(e) {
console.error('failed to check mmc parameter:' + e);
}
