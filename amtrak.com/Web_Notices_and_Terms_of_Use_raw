<script LANGUAGE="JavaScript">

function ShareShowOverlay(menuId) {
	if ($(menuId)) {
		if ($(menuId).style.display == "block") {
			$(menuId).style.display = "none";
		} else {
			$(menuId).style.display = "block";
		}
	}
}

function ShareHideOverlay(menuId) {
	if ($(menuId)) {
		$(menuId).style.display = "none";
	}
}

</script>


