<script>
//initialize jquery tabs plugin on new arrivals pages (for shopsense and related products)
if ( location.href.match( /newarrivals\.fly/g ) ) {
	$("#productRecommendationArea").tabs();
}
//add missing size script filter to search page
$(document).ready(function() {
	if ( location.href.match( /search\.fly/g ) ) {
		$('.filter-link').click(function() {
			window.location.href = $(this).attr('data-target');
		});
	}
});
</script>
     

