var optimizely = optimizely || [];
optimizely.push(["sc_svar", s_clven]);
optimizely.push("sc_activate");





// overrides for page-over-page prop, evar, event & listvar tracking
// NOTE: Code in the header takes the https: localStorage omniture values 
//       and resaves them to http: localStorage for access here
//       After saving the https values to http, the https values get flushed to prevent unintentional refires

(function() {
  if (window.CARSBRIDGE.getOmnitureBridgeValue('props')) {
    var o = window.CARSBRIDGE.getOmnitureBridgeValue('props');  
    for (var k in o) {
      s_clven[k] = o[k];
    }
    window.CARSBRIDGE.setOmnitureBridgeValue('props','');
  } 

  if (window.CARSBRIDGE.getOmnitureBridgeValue('evars')) {
    var o = window.CARSBRIDGE.getOmnitureBridgeValue('evars');  
    for (var k in o) {
      s_clven[k] = o[k];
    }
    window.CARSBRIDGE.setOmnitureBridgeValue('evars','');
  }

  if (window.CARSBRIDGE.getOmnitureBridgeValue('events')) {
    var o = window.CARSBRIDGE.getOmnitureBridgeValue('events');
    // events is a comma-delimited string, so be careful about additions
    if (s_clven.events) {
      s_clven.events = s_clven.events + "," + o;  
    } else {
      s_clven.events = o;
    }
    window.CARSBRIDGE.setOmnitureBridgeValue('events','');
  }

  if (window.CARSBRIDGE.getOmnitureBridgeValue('listvars')) {
    var o = window.CARSBRIDGE.getOmnitureBridgeValue('listvars');
    for (var k in o) {
      s_clven['list' + k] = o[k];
    }
    window.CARSBRIDGE.setOmnitureBridgeValue('listvars','');
  }

})();

/************* DO NOT ALTER ANYTHING BELOW THIS LINE ! ***************/
var s_code=s_clven.t();if(s_code) { document.write(s_code); };document.close();
</script>






















































	
	
	



  




  
   
   
   
  
  
  










































	
	
	
	









  
  
  
  
  
  
  


  
  
  
  
  
   
   
  
  
  
  

  
  

  
  

  
  
  
  


  

  	
  

  
	
  
  


  





 
 
 
 
 
  
  
  
 
 
 
  

  
  




 


