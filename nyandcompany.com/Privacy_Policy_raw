		<script charset="utf-8" type="text/javascript">
		function checkoutItemFromCart(eventName){
				var actionType = "updatecart";
				if (__cmbLoaded) {
					var action = TellApartCrumb.makeCrumbAction("nyefx6fdZKHW",actionType);
					if(eventName == 'removeAllItem'){
						action.setActionAttr("UpdateCartType","PartialRemove");
					}
					if(eventName == 'checkoutItem'){
						action.setActionAttr("UpdateCartType","Full");
					}
					
					//If determined that user is logged in, then get information from User Profile
					  action.setUserId("");
					action.finalize();
				}				
			}
		</script>
