	<script>
		var gptAds = {
    		adUnitZone: 'services',
  		    adUnitPath: '/4236/usweek.channels/services',
			keywords: [],
			isSponsored: function() {
    			var pathname = window.location.pathname;
    			return pathname.length > 11 && pathname.substring(pathname.length - 11, pathname.length) === 'Sponsorship';
			},
			leaderboardFooterLoaded: false,
			loadLeaderboardFooter: function() {
    			
    			if (!gptAds.leaderboardFooterLoaded) {
            		googletag.cmd.push(function() {
            
                        var slot = googletag.defineSlot(gptAds.adUnitPath, [[970,250], [970,126], [728, 91]], 'leaderboard_footer').addService(googletag.pubads()).setTargeting('pos', 'footer');
                        googletag.display('leaderboard_footer');
                        
                        try {
                            amznads.getAdsCallback('3050', function() {
                                amznads.setTargetingForGPTAsync('amznslots');
                                googletag.pubads().refresh([slot]);                     
                                // once the footer is loaded, prevent this function from firing again
                                gptAds.leaderboardFooterLoaded = true;
                            });
                            googletag.pubads().clearTargeting('amznslots');
                        } 
                        catch(e) {
                            googletag.pubads().refresh([slot]);                     
                            // once the footer is loaded, prevent this function from firing again
                            gptAds.leaderboardFooterLoaded = true;
                        }
                    });        
    			}                
			}
		};
		
		googletag.cmd.push(function() {

    		// header leaderboard
    		googletag.defineSlot(gptAds.adUnitPath, [[990,250], [990,125], [970,250], [970,125], [970,66], [728,90]], 'div-gpt-ad-1366039796875-0').addService(googletag.pubads());

							// right column, first spot
				googletag.defineSlot(gptAds.adUnitPath, [[300,250], [300,600]], 'div-gpt-ad-1366039796875-1').addService(googletag.pubads());
			            
            // tracking pixel
			googletag.defineSlot(gptAds.adUnitPath, [1,1], 'div-gpt-ad-1366039796875-2').addService(googletag.pubads());
			
			                // gallery sponsors
                googletag.defineSlot(gptAds.adUnitPath, [228,33], 'div-gpt-ad-1366039796875-3').addService(googletag.pubads());
						
			                // right column, second spot
    			googletag.defineSlot(gptAds.adUnitPath, [300, 251], 'div-gpt-ad-1366039796875-4').addService(googletag.pubads());
						
            							
						     
						
			googletag.defineOutOfPageSlot(gptAds.adUnitPath, 'div-gpt-ad-1366039796875-0-oop').addService(googletag.pubads());
            googletag.defineSlot(gptAds.adUnitPath, [4, 4], 'div-gpt-ad-4x4').addService(googletag.pubads());

            // page-level targeting, applies to all ad slots on the page
            if (gptAds.keywords.length) {
              googletag.pubads().setTargeting('kw', gptAds.keywords);
            }
			googletag.pubads().setTargeting('ksg', Krux.segments);
			googletag.pubads().setTargeting('kuid', Krux.user);
			googletag.pubads().setTargeting('khost', encodeURIComponent(location.hostname));
			
      		if (document.referrer.indexOf('facebook.com') > -1) {
    		    googletag.pubads().setTargeting('user', 'facebook');
      		}

            try {
              amznads.setTargetingForGPTAsync('amznslots');
            } catch(e) { /*ignore*/}

			googletag.pubads().enableSingleRequest();
			googletag.pubads().enableVideoAds();
			googletag.pubads().collapseEmptyDivs();
			googletag.pubads().setCentering(true);
			googletag.pubads().disableInitialLoad();
			googletag.enableServices();
		});

	</script>
