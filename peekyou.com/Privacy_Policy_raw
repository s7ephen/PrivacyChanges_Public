<script type="text/javascript">
var requests = new Array();
function add_reqs(req){
        var x = requests.length;
        requests[x]=req;
}
function stop_reqs(){
        var x = requests.length;

        for ( var i = 0; i<x; i++ )
        {
                requests[i].abort();
        }
}
</script>






