    <script>
        
      var resizeSmall = false;
     
             jQuery(window).resize(function(){
       
                    var current_width = jQuery(window).width();

                    if(current_width < 1650){
                        resizeSmall = true;
                         jQuery('.slider_container').css( "position", "absolute" );
                    }else{
                        resizeSmall = false;
                         jQuery('.slider_container').css( "position", "fixed" );
                    }
     
              });
             
             var width = jQuery(window).width();
               if(width < 1650){
                      resizeSmall = true;
               }else{
                   resizeSmall = false;
               }
        
        
        jQuery(function($) {
        
                var current_width = jQuery(window).width();

                if(current_width < 1650){
                    resizeSmall = true;
                     jQuery('.slider_container').css( "position", "absolute" );
                }else{
                    resizeSmall = false;
                     jQuery('.slider_container').css( "position", "fixed" );
                }

                var menuLeft_FB = document.getElementById( 'cbp-spmenu-fb' ),
                showLeftPush_FB = document.getElementById( 'showLeftPush_FB' ),
                menuLeft_TW = document.getElementById( 'cbp-spmenu-tw' ),
                showLeftPush_TW = document.getElementById( 'showLeftPush_TW' ),

                tw_btn = document.getElementById( 'tw_btn_sldr' ),
                fb_btn = document.getElementById( 'fb_btn_sldr' ),

                body = document.body;
                
                  showLeftPush_FB.onclick = function() {

                    _gaq.push(['_trackEvent', 'social_slider', 'clicked', 'fb']);

                    if( !($(".cbp-spmenu-open-tw").is(":visible") == true) ){
   
                        if(resizeSmall){
                            classie.toggle( body, 'cbp-spmenu-push-toright' );
                        }
                        
                        classie.toggle( menuLeft_FB, 'cbp-spmenu-open-fb' );   
                        
                        classie.toggle(tw_btn, 'open-btn' );   
                        classie.toggle(fb_btn, 'open-btn' );   
                        
                    }else{

                       jQuery("#cbp-spmenu-tw").removeClass("cbp-spmenu-open-tw");
                       
                       if(resizeSmall){
                            jQuery("body").removeClass("cbp-spmenu-push-toright");
                       }
                        
                       jQuery("#fb_btn_sldr").removeClass("open-btn");
                       jQuery("#tw_btn_sldr").removeClass("open-btn");
                       
                        if(resizeSmall){  
                            classie.toggle( body, 'cbp-spmenu-push-toright' );
                        }
                        
                       classie.toggle( menuLeft_FB, 'cbp-spmenu-open-fb' );   

                       classie.toggle(tw_btn, 'open-btn' );   
                       classie.toggle(fb_btn, 'open-btn' );     

                    }  
                  
                };
                
                showLeftPush_TW.onclick = function() {
            
                    _gaq.push(['_trackEvent', 'social_slider', 'clicked', 'tw']);
                    
                    if( !($(".cbp-spmenu-open-fb").is(":visible") == true) ){
                    
                        if(resizeSmall){
                            classie.toggle( body, 'cbp-spmenu-push-toright' );
                        }
                    
                    
                        classie.toggle( menuLeft_TW, 'cbp-spmenu-open-tw' );   
                      
                        classie.toggle(tw_btn, 'open-btn' );   
                        classie.toggle(fb_btn, 'open-btn' );  
                      
                    }else{

                        jQuery("#cbp-spmenu-fb").removeClass("cbp-spmenu-open-fb");
                     //   jQuery("body").removeClass("cbp-spmenu-push-toright");
                                
                        if(resizeSmall){
                            jQuery("body").removeClass("cbp-spmenu-push-toright");
                        }        
                                
                        jQuery("#fb_btn_sldr").removeClass("open-btn");
                        jQuery("#tw_btn_sldr").removeClass("open-btn");
                       
                     //   classie.toggle( body, 'cbp-spmenu-push-toright' );
                     
                        if(resizeSmall){  
                            classie.toggle( body, 'cbp-spmenu-push-toright' );
                        }
                     
                        classie.toggle( menuLeft_TW, 'cbp-spmenu-open-tw' );   
                        
                        classie.toggle(tw_btn, 'open-btn' );   
                        classie.toggle(fb_btn, 'open-btn' );  

                    }  
                 
                };
          });
     
    </script>      
    
    
     

