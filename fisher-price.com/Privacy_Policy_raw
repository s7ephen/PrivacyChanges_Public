          <script type="text/javascript">
              (function () {

            $("#main-nav-list >li >a").on('click', function (e) {
                $(this).toggleClass('expand');
                if (Modernizr.mq('all and (max-width: 1023px)')) {
                    e.preventDefault();
                }
            });
            $(".second-level-menus .subMenus-list .title").on('click', function () {
                $(this).parent().toggleClass('active');
                $(this).parent().find('ul').toggle();
                $(".second-level-menus").find('div.subMenus-list').not($(this).parent().show()).toggle(300);
                $("#main-nav-list").find('>li').not($(this).parent().parent().parent().parent().show()).toggle(300);
                $(this).parent().parent().parent().prev().toggle(300);
            });

            /*display for top Header Search Box*/
            $("#main-top-header .search-icon").on('click', function () {
                $("#searchBox .search-box").toggleClass("active");
            });
            $("#main-nav-list>li").hover(function () {
                var $el = $("#main-nav-list");
                // create delay to match the transition delay property in CSS (0.45 secs for mega menu hover)
                setTimeout(function () { $el.toggleClass("mm-active"); }, 150);
                $('#main-top-header .input-search').val("");
                $("#main-top-header .tt-menu").css("display", "none");
            });
        } ());
        $(document).ready(function () {

            hamburgerAnimate();
            subbrandLogoSwap();
            footerAccordion();

        });
        $(window).resize(function () {

            hamburgerAnimate();
            subbrandLogoSwap();
            footerAccordion();
        });
        /*Hamburger Animation START*/
        function hamburgerAnimate() {
            if (Modernizr.mq('all and (max-width: 1023px)')) {
                $(".navbar-toggle").on("click", function () {
                    $(this).toggleClass("active");
                    $el = $(this).siblings('.navbar-brand');
                    $(this).parents('#main-top-header').siblings(".main-navigation").toggleClass("mmactive");
                    $(this).hasClass("active") ? $el.text("Close") : $el.text("Menu");
                });
            }
        }
        /*Hamburger Animation END*/
        /*sub Brand Logo Swap Start*/
        function subbrandLogoSwap() {
            $el = $(".sub-brand-logo");
            //elemnt for product detail page hover text change
            if (Modernizr.mq('all and (max-width: 640px)')) {
                $el.insertAfter("#main-top-header .fp-logo");
            }
            else {
                $el.insertBefore(".main-navigation");
            }
        }
        function footerAccordion() {
            $('.footer-tiles .accordion-toggle h5').on('click', function () {
                $(this).toggleClass("shown");
            });
        }
    </script>        


