<script type="text/javascript">
function jpChristmasResize() {
	if(location.pathname == "/ja_JP/SpecialOffers/christmas.html") {
		$('iframe').css('transform-origin', '0 0');
		$('iframe').css('-ms-transform-origin', '0 0');
		$('iframe').css('-moz-transform-origin', '0 0');
		$('iframe').css('-o-transform-origin', '0 0');
		$('iframe').css('-webkit-transform-origin', '0 0');
		if($(window).width() < 520) {
			var scaleFactor = $(window).width() / 520;
			var inverseScaleFactor = 100 / scaleFactor;
			$('iframe').css('transform', 'scale(' + scaleFactor + ')');
			$('iframe').css('-ms-transform', 'scale(' + scaleFactor + ')');
			$('iframe').css('-moz-transform', 'scale(' + scaleFactor + ')');
			$('iframe').css('-o-transform', 'scale(' + scaleFactor + ')');
			$('iframe').css('-webkit-transform', 'scale(' + scaleFactor + ')');
			$('iframe').css('width', inverseScaleFactor + '%');
		}
		else {
			$('iframe').css('transform', 'scale(1)');
			$('iframe').css('-ms-transform', 'scale(1)');
			$('iframe').css('-moz-transform', 'scale(1)');
			$('iframe').css('-o-transform', 'scale(1)');
			$('iframe').css('-webkit-transform', 'scale(1)');
			$('iframe').css('width', '100%');
		}
	}
}

$(document).ready(function () {
	jpChristmasResize();
});

$(window).resize(function () {
	jpChristmasResize();
});

</script>
