		<script type="text/javascript">
			var findStoreActive = false;
			var inOverlay = false;
			var autocompleteWidget;
			var fasMouseX = 0;
			var fasMouseY = 0;
			
			function listenPacItems() {
				$(".pac-container").bind("mouseenter", function() {
					findStoreActive = true;
				});
				$(".pac-container").bind("mouseleave", function() {
					findStoreActive = false;
					setTimeout("hideFindStore()", 500);
				});
			}
			function hideSuggestions() {
				$(".pac-container").css("display", "none");
			}
			function showFindStore() {
				$("#storeContainer").css("display", "block");
				$("#storeContainer").css("right", "0");
				if($("#storeContainer").parent().prop("id") == "storebuttonarea") {
					$("#storeContainer").css("left", "-120px");
					$("#storeContainer").css("top", "26px");				
				} else {
					$("#storeContainer").css("left", "485px");
					$("#storeContainer").css("top", "117px");
				}
				var billingZip = getBillingZip();
				if(billingZip != '')
					document.getElementById('input_location').value = billingZip;
				
				findStoreActive = true;
				
				$("#input_location").keypress(function(e) {
					var location = $("#input_location").val();					
					if (e.keyCode == 13) {
						fsmFindStores(-1);
					} else if (location.length >= 4 && !isNaN(location)) {
						// Disable autocomplete for numeric entry to avoid international results
						for (var i = 0; i < 40; i++) {
							setTimeout("hideSuggestions()", i * 50);
						}
					} 
					setTimeout("listenPacItems()", 200);
				});
			}
			function hideFindStore(precedence) {
				//console.log("FINE: Enter hideFindStore");
				//console.log("FINER: findStoreActive: " + findStoreActive);
				//console.log("FINER: inOverlay: " + inOverlay);
				try {
					var fasX = $("#storeContainer").offset().left;
					var fasY = $("#storeContainer").offset().top;
					var hide = true;
					//console.log("FINER: fasX: " + fasX);
					//console.log("FINER: fasY: " + fasY);
					//console.log("FINER: fasMouseX: " + fasMouseX);
					//console.log("FINER: fasMouseY: " + fasMouseY);
					// Add or subtract 4 px to estimate position after mouseleave, since mousemove only tracks inside box
					if (fasMouseX - 4 < fasX && fasMouseY - 4 < fasY) {
						//console("FINER: Mouse outside FAS at top left");
					} else if ((fasMouseX + 4 >= fasX && fasMouseX - 4 <= (fasX + 380)) && fasMouseY - 4 < fasY) {
						//console.log("FINER: Mouse outside FAS at top");
					} else if (fasMouseX + 4 > fasX && fasMouseY - 4 < fasY) {
						//console.log("FINER: Mouse outside FAS at top right"); 
					} else if (fasMouseX + 4 > (fasX + 380) && (fasMouseY + 4 >= fasY && fasMouseY - 4 <= (fasX + 120))) {
						//console.log("FINER: Mouse outside FAS at right");
					} else if (fasMouseX + 4 > (fasX + 380) && fasMouseY + 4 > (fasY + 120)) {
						//console.log("FINER: Mouse outside FAS at bottom right");
					} else if ((fasMouseX + 4 >= fasX && fasMouseX - 4 <= (fasX + 380)) && fasMouseY + 4 > (fasY + 120)) {
						//console.log("FINER: Mouse outside FAS at bottom");
					} else if (fasMouseX - 4 < fasX && fasMouseY + 4 > fasY) {
						//console.log("FINER: Mouse outside FAS at bottom left");
					} else if (fasMouseX - 4 < fasX && (fasMouseY + 4 >= fasY && fasMouseY - 4 <= (fasX + 120))) {
						//console.log("FINER: Mouse outside FAS at left");
					} else {
						//console.log("FINER: Mouse inside FAS. Do not hide.");
						hide = false;
					}
					
					if (hide) {
						if (!precedence && !inOverlay) {
							if (!findStoreActive) {
								if (window.location.href.indexOf("StoreLocatorView") == -1) {								
									$("#storeContainer").hide();
								}
								$(".pac-container").hide();
								$("#auto-complete").hide();
							}
						} else if (precedence) {
							if (!findStoreActive && !inOverlay) {
								if (window.location.href.indexOf("StoreLocatorView") == -1) {
									$("#storeContainer").hide();
								}
								$(".pac-container").hide();
								$("#auto-complete").hide();
							}
						}
					}					
				} catch (e) {
					//console.log("ERROR: " + e);
				}
			}			
		
			$(document).ready(function() {
				if (window.location.href.indexOf("StoreLocatorView") == -1) {
					var timer;
				
					$("#storebuttonarea").mouseover(function() {
						timer = setTimeout("loadScript()", 500);
					});					
					$("#storebuttonarea").mouseout(function() {
						clearTimeout(timer);
						if (findStoreActive) {
							findStoreActive = false;
							setTimeout("hideFindStore(false)", 500);
						}
					});
				
					$("#storeContainer").bind("mouseenter", function() {
						
						$("#storeContainer").bind("mousemove", function(e) {				
							fasMouseX = e.pageX;
							fasMouseY = e.pageY;
						});
											
						findStoreActive = true;
						inOverlay = true;
					});
				
					$("#storeContainer").bind("mouseleave", function() {				
						
						$("#storeContainer").unbind("mousemove");	
						
						findStoreActive = false;
						inOverlay = false;
						setTimeout("hideFindStore(true)", 500);
					});
				}
				
				$("#storeContainer").bind("mouseleave", function() {					
					$("#auto-complete").hide();
				});				
				
			});
			
			
		function getCookie(name) {

			var cookies = document.cookie.split(';');
			var value   = null;	
			for ( var i=0; i < cookies.length; i++ ) {
				var cookie = cookies[i];		
				while ( cookie.charAt(0) == ' ' ) {
					cookie = cookie.substring(1, cookie.length );
				}
				if ( cookie.indexOf(name+"=") == 0 ) {
					value = cookie.substring(name.length+1, cookie.length);
					value =	unescape(value);
				}
			}
			return value;
		}
		$(document).ready(function () {
		     var cjParameter = '';
		     var cjCookieValue = getCookie("cjReq_"+cjParameter);
		     if(cjCookieValue == null && cjParameter != null && cjParameter.length > 0) {
		         SetCJCookie("cjReq_"+cjParameter, cjParameter);
		     }
		  });
		
		 function SetCJCookie (name, value) {
			var defaultexp = new Date ();
			if(name.startsWith("cjReq_")) { 
				//extend cj cookie expiration to 30 days
				// 08/06/2015 Seth Jackson ECOMDEV-19417 - Changed cookie duration back to 1 day
				defaultexp.setTime (defaultexp.getTime() + (24 * 60 * 60 * 1000));	
			}
			else {
				defaultexp.setTime (defaultexp.getTime() + (24 * 60 * 60 * 1000));
			}
			
			var path =  "/";
			var domain = null;
			var secure = false;
			document.cookie = name + "=" + escape (value) +  ((defaultexp == null) ? "" : ("; expires=" + defaultexp.toGMTString())) + ((path == null) ? "" : ("; path=" + path)) + ((domain == null) ? "" : ("; domain=" + domain)) + ((secure == true) ? "; secure" : "");
		}
			
