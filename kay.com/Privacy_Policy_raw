<script language='JavaScript'>
// setting the intial sdl variables to blank -->
var	sdl_OrderId = '';
var	sdl_StoreId = '';
var sdl_UsersId = '';
var sdl_Email= '';
var sdl_totalAmout = '';
var sdl_subTotal = '';
var sdl_totalAmountTax = '';
var sdl_totalShipping = '';
var sdl_currentProductTotal = '';
var sdl_city = '';
var sdl_state = '';
var sdl_zip = '';
var sdl_shipZip = '';
var sdl_country = 'USA';
var sdl_quantity = '1';
var sdl_currency = 'USD';
var sdl_orderItems = new Array();
var sdl_orderItem = new Object();
// 07/28/2015 Seth Jackson ECOMDEV-19412 - Added new SDL array for all discount codes on the order
var sdl_discountCodes = new Array();
var sdl_pageName = 'ContentView';
if (sdl_pageName == 'CategoryDisplay' || sdl_pageName == 'CategoryProducts' || sdl_pageName == 'SterlingSearch' || 
		sdl_pageName == 'SearchResults') {
		var sdl_creditCookie = getCookie('OTBAMOUNT10101');
		if (sdl_creditCookie == null) {
			sdl_creditCookie = '-1';
		}
	}
sdl_orderItem.sku = '';
sdl_orderItem.adjustedPrice = '';
sdl_orderItem.retailPrice = ''; //TODO
var sdl_Discount_AMT='';//Discount variable
sdl_orderItem.Discount_AMT='';//Discount variable
sdl_orderItem.description = '';
sdl_orderItem.category = '';
sdl_orderItem.productClass='';


var sdl_recency = '';

//Set the ShopRunner Data to blank.
var sdl_shopRunnerToken = '';
var sdl_tenderType = '';


// setting Google data layer to blank 
var gtm_pagetype = '';
var gtm_pcat = '';
var gtm_prodid = '';
var gtm_pname = '';
var gtm_pvalue = '';

		



// Google Data Layer 
dataLayer = [];

var prodid_array = []; 
var pname_array = [];
var pcat_array = [];
var value_array =[];
if( sdl_orderItems != null && sdl_orderItems.length > 0 ) {
	for (var i = 0; i < sdl_orderItems.length; i++) { 
	  if( sdl_orderItems[i].adjustedPrice && sdl_orderItems[i].adjustedPrice != null ) {
		  sdl_orderItems[i].adjustedPrice = sdl_orderItems[i].adjustedPrice.replace('$','');
		  sdl_orderItems[i].adjustedPrice = sdl_orderItems[i].adjustedPrice.replace(',','');
	  }		
	  prodid_array.push(sdl_orderItems[i].sku); 
	  pname_array.push(sdl_orderItems[i].description); 
	  pcat_array.push(sdl_orderItems[i].category); 
	  value_array.push(sdl_orderItems[i].adjustedPrice);	  
	} 
} else {
  prodid_array.push(gtm_prodid); 
  pname_array.push(gtm_pname); 
  pcat_array.push(gtm_pcat); 
  value_array.push(gtm_pvalue); 
}

dataLayer.push(
  { 
  prodid: prodid_array, 
  pagetype: gtm_pagetype, 
  pname: pname_array, 
  pcat: pcat_array, 
  value: value_array  
  } 
);

if( gtm_pagetype && gtm_pagetype != null && gtm_pagetype == 'purchase' ) {
  var gtmTransactionTotal = 0;
  var gtmTransactionProducts = [];
  if( sdl_orderItems != null && sdl_orderItems.length > 0 ) {
     for( var i = 0; i < sdl_orderItems.length; i++ ) {
       var gtmAdjustedPrice = sdl_orderItems[i].adjustedPrice;
       if( gtmAdjustedPrice && gtmAdjustedPrice != null ) {
    	   gtmAdjustedPrice = gtmAdjustedPrice.replace('$','');
    	   gtmAdjustedPrice = gtmAdjustedPrice.replace(',','');
       }
       if( jQuery.isNumeric(gtmAdjustedPrice) ) {
         gtmTransactionTotal = gtmTransactionTotal + Number(gtmAdjustedPrice);
       }
       
       gtmTransactionProducts.push(
         {
           'sku': sdl_orderItems[i].sku,
           'name': sdl_orderItems[i].description,
           'category': sdl_orderItems[i].category, 
           'price': gtmAdjustedPrice, 
           'quantity': '1'
         }
       );
     }    
  }
  var gtmShipping = 0;
  if( sdl_totalShipping && sdl_totalShipping != null ) {
	  gtmShipping = sdl_totalShipping.replace('$','');
  }
  

  dataLayer.push({
	'transactionId': sdl_OrderId,
	'transactionAffiliation': sdl_StoreId,
	'transactionTotal': gtmTransactionTotal,
	'transactionTax': sdl_totalAmountTax,
	'transactionShipping': gtmShipping,
	'transactionProducts': gtmTransactionProducts
	});
}

