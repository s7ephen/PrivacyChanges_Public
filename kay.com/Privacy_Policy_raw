 function unhide(divID) {
	 var item = document.getElementById(divID);
	 if (item) {
	 	item.className=(item.className=='hidden')?'unhidden':'hidden';
	 }
 }


 function toggle_visibility(id) {
    var e = document.getElementById(id);
    if(e.style.display == 'block')
       e.style.display = 'none';
    else
       e.style.display = 'block';
 }
 
 function toggle_visibility(visibleId,hiddenId) {
    var visible = document.getElementById(visibleId);
    var hidden = document.getElementById(hiddenId);
    if(visible != null && hidden != null){
    	visible.style.display = 'none';
	    hidden.style.display = '';
    }
}

function getStoreNumber(){
	var number = getCookie('STORENUMBER10101');
	return number;
}
function getCookie(name) {

	var cookies = document.cookie.split(';');
	var value   = null;

	for ( var i=0; i < cookies.length; i++ ) {
		var cookie = cookies[i];

		while ( cookie.charAt(0) == ' ' ) {
			cookie = cookie.substring(1, cookie.length );
		}

		if ( cookie.indexOf(name+"=") == 0 ) {
			value = cookie.substring(name.length+1, cookie.length);
			value =	unescape(value);
		}
	}

	return value;
}
function getDisplayName() {
	var name = getCookie( "DISPLAYNAME" );
	if ( name == null ) {
		name = "guest";
	}
	return name;
}

function getShopCart(){
	var c = getCookie('SHOPPINGCART10101');
	var mcart = (c != null) ? eval("(" + c + ")") : null;
	var quantity = (mcart != null) ? mcart.quantity : 0;
	//alert("mcart : " + mcart);
	//alert("quantity : " + quantity);
	return quantity;
}
function getOTBAmount(){
	var c = getCookie('OTBAMOUNT10101');
	if (c==null){
		c = 0.00;
	}
	return c;
}

function getBillingZip(){
	var c = getCookie('BILLINGZIP10101');
	if (c==null){
		c = '';
	}
	return c;
}

function creditAppWindow(url) {
		openWindow(url, "credit_app", 800, document.body.clientHeight, "scrollbars=yes,menubar=no,toolbar=no,location=no,status=yes,resizable=yes");
	}
	
function search(){

 	var storeId='10101';
  	var catalogId='10001';
    var langId='-1';
    var searchTerm = document.globalSearchForm.Ntt.value;
    var ntk = document.globalSearchForm.Ntk.value;
    var n=document.globalSearchForm.N.value;
     var ntt=document.globalSearchForm.Ntt.value;
    var ne=document.globalSearchForm.Ne.value;
	  var url = 'search|'+storeId+'|'+catalogId+'|'+langId+'|'+searchTerm+'|'+n+'|'+ne+'|'+ntk+'|'+ntt;
	 
  //   var url = 'SearchResultsView?langId=-1&storeId=10451&catalogId=10001&N=0&Ne=1&Ntk=Products&Ntt=Levian&searchButton.x=0&searchButton.y=0';
    document.location.href = url;
  
 }


</script>

<script>
		// Activate ratings and reviews tab in PIP when user clicks on ratings icon in other pages.
		$(document).ready(function () {
            var reviewLoc = window.location.href.indexOf("reviews");
            //console.log(window.location.href);
            if ($('.pip-tabset-container').html()) {  
            if(reviewLoc > 0) {
               $('.pip-tabset-container .tabset .tab, .pip-tabset-container .tab-content-container .tab-content').removeClass('active');
               $('.pip-tabset-container .tabset .tab[id=tab3]').addClass('active');
               $('.pip-tabset-container .tab-content-container .tab-content[id=tab3]').addClass('active');
               $(window).scrollTop($('.pip-tabset-container').position().top);  
            }
			}  
      });
</script>

