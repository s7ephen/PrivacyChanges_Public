			<!-- Declare messages/errors in this page. We can access these variable in javascript/jQuery where ever required -->
<script>
	var emptyFields = "ATTENTION!: The highlighted fields are required. Please enter the information.";
	var fromEmailEmpty = "ATTENTION! Please enter a valid email address.";
	var fromEmailInvalid = "ATTENTION! Please enter a valid email address."; 
	var toEmailEmpty = "ATTENTION! Please enter a valid email address.";
	var toEmailInvalid = "ATTENTION! Please enter a valid email address.";
	var captchaEmpty = "ATTENTION! Please enter the required security phrase shown in the box.";
	var captchaInvalid = "ATTENTION! The security phrase you entered is incorrect.  Please re-enter the required security phrase shown in the box.";
	var emailSuccess = "Thank you, your email has been sent.";
	var emailFailed = "ATTENTION! We are sorry, your email could not be not sent due to technical issues. Please contact Customer Care for assistance.";
	var publicKey = "6LefO8sSAAAAADkDnaU_iotABC7riNwDTB_4oXv5 ";
</script>

<style>
/* positioning changed on this file to make the popups display in the correct location */
	.content { position : static;}
</style>

<script> 
	var rc = document.createElement("script");rc.type = "text/javascript";rc.async = true;
	rc.src = "//www.google.com/recaptcha/api/js/recaptcha_ajax.js";
	var s = document.getElementsByTagName("script")[0];s.parentNode.insertBefore(rc, s);

	$(function() {
		//select all the a tag with name equal to modal
		$('a[name=modal]').click(function(e) {
		
		var itemId_param = document.getElementById("emailItem");
		if(itemId_param != null && itemId_param != "undefined") {
		document.getElementById("itemId").value =  itemId_param.value;
		}
             loadEmailModal($(this).attr('href'));
		});
		
		// To populate cms title as subject in the email popup for content pages.
		var cmsTitle = document.getElementById("cmsTitle");
		if(cmsTitle != null && cmsTitle != "undefined") {
			var contentViewLoc = window.location.href.indexOf("ContentView");
			if(contentViewLoc > 0) {
				document.getElementById("subject").value = cmsTitle.value;
			}
		}
		
	});
	
	function loadEmailModal(dialogId){
	
	// To hide error display div when there is no error.
			$("#errorSub").hide();
			var userType = 'G';
			//Cancel the link behavior
			//e.preventDefault();
			
			//Get the A tag
			var id = dialogId;
			//Get the screen height and width
			var maskHeight = $(document).height();
			var maskWidth = $(window).width();
		
			//Set heigth and width to mask to fill up the whole screen
			$('#emailMask').css({'width':maskWidth,'height':maskHeight});
			
			//transition effect		
			$('#emailMask').fadeIn(1000);
			$('#emailMask').fadeTo("slow",0.8);
			 
			//Set the popup window to center
			$(id).css("position","absolute");
			$(id).css("top", ( $(window).height() - $(id).height() ) / 2 + $(window).scrollTop() + "px");
			$(id).css("left", ( $(window).width() - $(id).width() ) / 2 + $(window).scrollLeft() + "px");	 		
		
			//transition effect
			$(id).fadeIn(2000); 
	
			// new captcha everytime email popup opens. 
			reloadCaptcha("recaptcha_div");
			// ADA 2014 handles keyboard, keeping focus within the dialog
			$("#dialog1").on ("keydown", modalKeyboardHandler); 
	}
	
	// ADA 2014 This function keeps keyboard focus within the dialog; escape to cancel...
	function modalKeyboardHandler (e) {
		var $dialog = $(this);
		if ($dialog.is(":visible")) {
			// find first and last focusable items in the dialog
			var first = $('#dialog1 .close');
			var firstId = "closeBtn";
			var last = $('#dialog1 .modalInput:last');
			var lastId = "send";
			
			// keep tabs and escapes from propagating outside this dialog
			e.stopImmediatePropagation();
			e.stopPropagation();
			switch (e.keyCode || e.which) {
				case 27: // escape
					return false;
					break;
				
				case 9:  // shift+tab
					if (e.shiftKey) {
						if (e.target.id == firstId) {
							// if we fall off the beginning, wrap to end
							e.preventDefault();
							last.focus();
							return false;
						}
						break;
					}
				
				case 9: // tab key without shift
					if (e.target.id == lastId) {
						// if we fall off the end, wrap to beginning
						e.preventDefault();
						first.focus();
						return false;
					}
			}
		}
		return true;
	}
</script>

