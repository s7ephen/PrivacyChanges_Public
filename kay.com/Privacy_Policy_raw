<script language="javascript">
	var isCheckOut = '';
	var displayName = getDisplayName();
	var storeNumber = getStoreNumber();
	var loggedIn = false;
	if ( displayName == "guest"  ) {
	  loggedIn = false;
	} else {
	  loggedIn = true;
	}
	var creditAmt = getOTBAmount();
	var creditDen = null;
	if (loggedIn) {	
		if (storeNumber != 'null' && storeNumber != null){
			document.write('<div class="accent">Welcome Store  '+storeNumber +'</div>');
			//var apply="Apply for Kay Credit";
			//document.write('<div class="accent"><a href="#" onclick="creditAppWindow(\'CreditApplicationView?langId=-1&amp;storeId=10101&amp;catalogId=10001&amp;applyLoc=\')">'+ "<span class=\"link\">" + apply + "</span>" +'</a></div>');
		}else{
			document.write('<span class="accent">Hi ' + displayName+ ',&nbsp;</span>');
			document.write('<a class="link" href="/Logoff?storeId=10101&amp;catalogId=10001&amp;langId=-1&amp;URL=LogonForm" tabindex="10">Not&nbsp;'+displayName+'?</a>');
			document.write('<span class="spacer">&nbsp;&#124;</span>');
			document.write('<a class="link" href="/Logoff?storeId=10101&amp;catalogId=10001&amp;langId=-1&amp;URL=LogonForm" tabindex="20">Sign Out</a>');
			if (parseInt(creditAmt) > 0){  // case when the customer is registered and has non-zero Sterling Account OTB
				document.write('<span id = "hideCredit" style="display:"><span class="accent credit">Available Credit: $' + creditAmt+'</span><span class="spacer">&nbsp;&#124;</span><a class="link" href="#" onclick="toggle_visibility(\'hideCredit\',\'viewCredit\'); return false;" tabindex="30">Hide</a></span>');
				document.write('<span id = "viewCredit" style="display:none"><a onmouseover=\'this.style.cursor="pointer" \' onfocus=\'this.blur();\' onclick="toggle_visibility(\'viewCredit\',\'hideCredit\');" href="#" class="accent credit" tabindex="40">View Your Available Credit</a></span>');
				//document.write('<a href="#" onclick="toggle_visibility(\'otbamount\'); return false;">View your Available Credit</a>');
			}else if(creditDen == true){ //case when customer is registered and his account is under review or Denied, Apply for credit should not be displayed.
			}else if(parseInt(creditAmt) == 0){  // case when customer is registred and has no credit account associated
		   	     var apply="Apply for Kay Credit";
				 document.write('<div class="accent credit"><a href="#" onclick="creditAppWindow(\'/CreditApplicationView?langId=-1&amp;storeId=10101&amp;catalogId=10001&amp;applyLoc=\')" tabindex="50">'+ "<span class=\"link\">&nbsp;" + apply + "</span>" +'</a></div>'); 
			}else{  // case when the customer has sterling account registered but the OTB ==0 or less than threshhold. 
			
			}
		}				
	}else{ // guest user
		document.write('<a class="link" href="/LogonForm?langId=-1&amp;storeId=10101&amp;catalogId=10001&amp;errorViewName=LogonForm" tabindex="20">Sign In</a> <span class="spacer">&nbsp;&#124;&nbsp;</span>'); 
		var apply="Apply for Kay Credit";
		//document.write('<a class=\"link\" href="#" onclick="creditAppWindow(\'CreditApplicationView?langId=-1&amp;storeId=10101&amp;catalogId=10001&amp;applyLoc=\')">'+  apply + '</a>');
		document.write('<div class="accent credit_guest"> <span class="sprite sprite-66credit-small"></span> <a class=\"link\" href="#" onclick="creditAppWindow(\'/CreditApplicationView?langId=-1&amp;storeId=10101&amp;catalogId=10001&amp;applyLoc=\')" tabindex="50">'+  apply + '</a></div>'); 
	}
	if((window.location.port!='null' && window.location.port!='undefined') && (window.location.port=='8006' || window.location.port=='8007')){
		document.write('<span class="spacer"> | </span><a class="link" href="http://www.kay.com/MobileEspotDisplayView?langId=-1&amp;storeId=10101&amp;catalogId=10001" tabindex="20">Mobile E-Spots (preview)</a>');
	}
</script>


