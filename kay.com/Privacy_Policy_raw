<script type="text/javascript">
function getDelimitedListForDataLayerItem(name, delimiter) {
  	      
  	      var result = ""

		  if( name && name != null && name != "" &&
		      delimiter && delimiter != null && delimiter != "") {
			  if( sdl_orderItems != null && sdl_orderItems.length > 0 ) {
					for( var i = 0; i < sdl_orderItems.length; i++ ) {
					  var _val = "";
					  if( name == "sku" ) {
					    _val = sdl_orderItems[i].sku;
					  } else if( name == "adjustedPrice" ) {
					    _val = sdl_orderItems[i].adjustedPrice;
					  } else if( name == "description" ) {
					    _val = sdl_orderItems[i].description;
					  } else if( name == "category" ) {
					    _val = sdl_orderItems[i].category;
					  } else if( name == "productClass" ) {
					    _val = sdl_orderItems[i].productClass;
					  } else if( name == "quantity" ) {
					    _val = "1";
					  } else if( name == "rating" ) {
					    _val = sdl_orderItems[i].rating;
					  }
		
					  // remove invalid characters
					  if( typeof _val === "undefined" || _val == null ) {
					    _val = "";
					  }
					  _val = _val.replace(',','');
					  _val = _val.replace('$','');
		
					  if( result == '' ) {
						result += _val;
					  } else {
						result += delimiter + _val;
					  }
					}
			  }
		  }
		  return result;
		}
</script>
	

