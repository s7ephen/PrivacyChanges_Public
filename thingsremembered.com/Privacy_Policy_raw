    <script type="text/javascript">
            function createStoreInfo() {
                var url = '/custserv/store_locator_layer.jsp';
                $.ajax({
                    type: "POST",
                    url: url,
                    dataType: "html",
                    timeout: 15000,
                    success: function (data) {
                        $('#store-locator-container').html(data);
                        return true;
                    },
                    error: function () {
                        return false;
                    }
                });
            }

        </script>
