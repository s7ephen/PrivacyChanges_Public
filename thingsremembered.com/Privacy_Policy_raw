		<script language="Javascript" type="text/javascript">

            $(".links-dropdown.active").click(function() {
                var dropDownContent = $(this).next(".dropdown-content");
                $("#store-locator-layer-wrap").hide();
                if(dropDownContent.is(":hidden")) {
                    dropDownContent.show();
                } else {
                    dropDownContent.hide();
                }
            });
            $("#widget-but-ucart").click(function(){
                window.location = '/checkout/basket.jsp';
            });

			var omniturexhr  = $.ajax({
		      type: "GET",
	         url: '/js/omniture.js',
	         dataType: "script",
	         cache: true,
	         success: function() {
					omniturexhr = null;
				}
			});
			setTimeout(function(){
               // If the request is still running, abort it.
		         if (omniturexhr) omniturexhr.abort();
				}, 2500
			);
		</script>
