<script type="text/javascript">
        $(function () {
            var ucartTimer;

            //$("#widget-but-ucart").hover(function(){
            $(".cart-info-container").hover(function () {
                ucartTimer = setTimeout(function () {
                    showBasket('show', '');
                }, 500);
            }, function (ev) {
                if (typeof ucartTimer != undefined) {
                    clearTimeout(ucartTimer);
                }
            });
        });

        // When the Universal Cart layer opens
        // it will close if the user clicks on anything in the main browser window
        $(document).click(function (event) {
            var posX = event.clientX;
            var posY = event.clientY;
            var miniCart = $('#widget-ucart');
            var miniCartPos = miniCart.offset();
            if (isShowingBasket()) {
                // If user click to background, close the mini cart
                if (!(posX >= miniCartPos.left && posX <= (miniCartPos.left + miniCart.width())
                        && posY >= miniCartPos.top && posY <= (miniCartPos.top + miniCart.height()))) {
                    hideBasket();
                }
            }
        });
    </script>
<script type="text/javascript">
    $(document).ready(function () {
        getHomeStore();
    })

</script>



<script type="text/javascript">
	function callCheetahPreferencesFromFooter(){
		var userEmailFooter = $("#userEmailFooter").val();		
		if (trim(userEmailFooter) != '')
			$("#userEmail").val(userEmailFooter);		
		else
			$("#userEmail").val('Enter your email address');		
		var subscribeForm = document.subscribeForm;	    
		subscribeForm.submit();		
	    
	}
</script>

<script type="text/javascript">
  	$(document).ready(function() {
		$(".email-signup-contianer #userEmailFooter").val('Enter your email address');
		var userEmail;

		$(".email-signup-contianer #userEmailFooter").focus(function(){
			userEmail = $(this).val();
			if (userEmail == 'Enter your email address' || trim(userEmail) == ''){
				$(this).val('');
			}
		});
		
		$(".email-signup-contianer #userEmailFooter").blur(function(){
			userEmail = $(this).val();
			if (trim(userEmail) == '' || userEmail == 'Enter your email address'){
				$(this).val('Enter your email address');
			}
		});
	});
</script>
