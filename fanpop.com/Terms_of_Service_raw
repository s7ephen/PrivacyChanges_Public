<script type="text/javascript">
$('.mobile-menu-button').bind('click.menu', function(e) {
		var menu = $('nav.menu');
		if (menu.length > 0) {
			if (menu.hasClass('hidden')) {
					menu.removeClass('hidden');
			} else {
					menu.addClass('hidden');
			}
		}
});
</script>



		
