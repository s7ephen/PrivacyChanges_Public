//<![CDATA[
// External site redirection pop up functions
var externalSites = 'www.jcpenneyoptical.com,www.jcpportraits.com,www.jcp.jodee.com,www.teleflora.com,www.stonebridgebenefit.com,www.perfecthomerewards.com,www.stonebridge-jcp.com,www.sephora.com,www.jcpenney.com/jcp/x2.aspx?DeptID=83168&amp;CatID=83168,www.jcpenney.com/jcp/X2.aspx?DeptID=78785&amp;PCatID=78785&amp;CatID=78785'.split(',');
var syndicateSites = 'levolortest.jcpenney.com,baliblinds.jcpenney.com,sportsfanshoptest.jcpenney.com,personalizedjewelrytest.jcpenney.com,levolor.jcpenney.com,baliblinds.jcpenney.com,sportsfanshop.jcpenney.com,personalizedjewelry.jcpenney.com'.split(',');
var isRetailMode = false;
function submitForm(){
if (noValueEnteredMessage('searchTerm')) {
submitSearchForm();
showSpinner('bodyWrapper');
}
return false;
}
function submitSearchForm() {
$('#nttHolderId').val($('#searchTerm').val());
showSpinnerForClass("holder");
var options = {
url: '/dotcom/jsp/search/searchSubmit.jsp?' + $('#keywordSearch').serialize() + '&srh_sub=',
type: 'GET',
dataType: 'json',
cache: true,
success : submitSearchFormSuccess
//error : errorLeavingJCPModal
};
$('#keywordSearch').ajaxSubmit(options);
}
function submitSearchFormSuccess (responseText)
{
hideSpinner();
var redirectUrl = (responseText.redirectUrl).replace(/\&amp;/g, '&');
if (isRetailMode) {
window.location.href = redirectUrl;
} else {
var isOpenModal = displayLeavingJCPModal(redirectUrl);
if(isOpenModal) {
var href = $('#bumper').attr("href");
newHref = href+"targetSite="+redirectUrl;
$('#bumper').attr("href", newHref);
$('#bumper').click();
} else {
window.location.href = redirectUrl;
}
}
}
function displayLeavingJCPModal(linkPath) {
// If Shopping Bag is empty no need to show "leaving jcpenney.com" modal.
var orderCount = getCookie("ItemCount");
if(orderCount == undefined || orderCount == null || orderCount == '' || orderCount == 0)
return false;
// Checking if the redirect is to external site or not
if(urlContains(linkPath, externalSites)) {
return true;
}
}
function handleEnterKeyForSearch(e, searchButtonId) {
var key;
if(window.event) {
key = window.event.keyCode; //IE
} else {
key = e.which; //firefox
}
//13 is ASCII code for Enter key
if(key != 13) {
return true;
} else {
submitForm();
return false;
}
}
function handleClickForSerach(searchButtonId) {
$(".jcp_search .header_search .search_input").css("color", "#000000");
var temp = $("#"+searchButtonId).val();
if(temp == "enter search term or web id"){
$("#"+searchButtonId).val("");
}
}
function handleBlurForSerach(searchButtonId){
$(".jcp_search .header_search .search_input").css("color", "#cdcdcd");
var temp = $("#"+searchButtonId).val();
if(temp == ""){
$("#"+searchButtonId).val("enter search term or web id");
} else {
$(".jcp_search .header_search .search_input").css("color", "#000000");
}
}
