<script type="text/javascript">
function renderCoreMerticPageViewTag(pageId, categoryId) {
var clientId = '90024642';
var renTagUrl = 'www88.jcpenney.com';
var renTagDomain = 'jcpenney.com';
var orderVid = '';
var searchTerm = '';
searchTerm = removeEncodedSpecialChar(searchTerm);
var searchResults = '';
if(orderVid == null)
orderVid = '';
var mmcParamArray = getMarketingAttributeFromCookie();
cmSetClientID(clientId,false,renTagUrl,renTagDomain);
cmSetupOther({"cm_FormPageID":true});
var userState = getCookie('DP_USER_STATE');
var typeOfCheckoutCm31 = "";
var isExpressCheckout = 'false';
if(userState == '1'){
if (isExpressCheckout == 'true') {
typeOfCheckoutCm31 = "authenticated customer_express checkout";
} else {
typeOfCheckoutCm31 = "authenticated customer_regular checkout";
}
} else {
typeOfCheckoutCm31 = "guest checkout";
}
var attributes ='-_-'+'-_-'+'-_-'+orderVid+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+typeOfCheckoutCm31+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+mmcParamArray[0]+'-_-'+mmcParamArray[1]+'-_-'+mmcParamArray[2]+'-_-'+mmcParamArray[3];
var extra='-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+orderVid;
cmCreateManualPageviewTag(pageId, categoryId,window.location.href,document.referrer,attributes,searchTerm,searchResults,extra);
}
</script>
