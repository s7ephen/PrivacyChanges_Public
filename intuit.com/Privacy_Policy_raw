<script type="text/javascript">
function renderCoreMerticPageViewTag(pageId, categoryId) {
var clientId = '90024642';
var renTagUrl = 'www88.jcpenney.com';
var renTagDomain = 'jcpenney.com';
var orderVid = '';
var searchTerm = '';
searchTerm = removeEncodedSpecialChar(searchTerm);
var searchResults = '';
if(orderVid == null)
orderVid = '';
var mmcParamArray = getMarketingAttributeFromCookie();
cmSetClientID(clientId,false,renTagUrl,renTagDomain);
cmSetupOther({"cm_FormPageID":true});
var userState = getCookie('DP_USER_STATE');
var typeOfCheckoutCm31 = "";
var isExpressCheckout = 'false';
if(userState == '1'){
if (isExpressCheckout == 'true') {
typeOfCheckoutCm31 = "authenticated customer_express checkout";
} else {
typeOfCheckoutCm31 = "authenticated customer_regular checkout";
}
} else {
typeOfCheckoutCm31 = "guest checkout";
}
var attributes ='-_-'+'-_-'+'-_-'+orderVid+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+typeOfCheckoutCm31+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+mmcParamArray[0]+'-_-'+mmcParamArray[1]+'-_-'+mmcParamArray[2]+'-_-'+mmcParamArray[3];
var extra='-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+orderVid;
cmCreateManualPageviewTag(pageId, categoryId,window.location.href,document.referrer,attributes,searchTerm,searchResults,extra);
}
</script>
<script type="text/javascript">
function renderCoreMerticPageViewTagonGR(pageId, categoryId, grView, searchString, searchResults, searchCount) {
var orderVid = '';
if(orderVid == null)
orderVid = '';
var mmcParamArray = getMarketingAttributeFromCookie();
var attributes ='-_-'+'-_-'+'-_-'+orderVid+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+mmcParamArray[0]+'-_-'+mmcParamArray[1]+'-_-'+mmcParamArray[2]+'-_-'+mmcParamArray[3];
var extra='-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+categoryId+'-_-'+'-_-'+'-_-'+orderVid;
var clientId = '90024642';
var renTagUrl = 'www88.jcpenney.com';
var renTagDomain = 'jcpenney.com';
cmSetClientID(clientId,false,renTagUrl,renTagDomain);
cmSetupOther({"cm_FormPageID":true});
cmCreateManualPageviewTag(pageId, categoryId,window.location.href,document.referrer,attributes,searchString,searchResults,extra);
}
</script>
<script type="text/javascript">
$("#showHide").css('display', 'block');
$("#slidingDiv").hide();
$('#showHide').click(function(){
$('#showHide span').toggleClass('toggleArrowNormal');
$("#slidingDiv").slideToggle('fast', function(){
$('#showHide span').toggleClass('toggleArrowOpen');
});
});
//move link CA transparency to about us per DP-4520 comp
$(document).ready(function(){
$("#optinEmail").val("enter email address");
$("#optinMobileno").val("enter phone number");
var allElem = $(".ft_customer_service ul li");
for (var i = 0, iLen = allElem.length; i < iLen; i++) {
if ($(allElem[i]).find("a").text().indexOf("CA transparency in supply chain") !== -1) {
$(allElem[i]).remove();
}
}
$('#descriptionTextCopy table').height('100%').css('margin-bottom', '12px');
});
</script>


