<script type="text/javascript">
var renderTagPageId = "";
var renderTagCategId = "";
var previousPageId = "";
var currentDimNameOfMKL="";
//<![CDATA[
function renderCoreMertic(jcp_t,jcp_c,jcp_m,cm_vc) {
if (jcp_c != '' && jcp_c.charAt(jcp_c.length-1) == '/') {
jcp_c = jcp_c.substr(0, jcp_c.length-1);
}
var cmRe15 = '';
if ( cm_vc && cm_vc != '') {
var coreMetricsCatId = cm_vc;
var cmRRZoneIndex = coreMetricsCatId.indexOf("|cmRRZone-");
if ( cmRRZoneIndex != -1 ) {
cm_vc = coreMetricsCatId.substr(0,cmRRZoneIndex);
var cmRRZoneAttr = coreMetricsCatId.substr(cmRRZoneIndex+1);
cmRe15 = cmRRZoneAttr.split('-')[1];
}
}
//Start DPPROG-1423 setting coremetrics Host for POSF
//END DPPROG-1423 setting coremetrics Host for POSF


renderTagPageId = 'Welcome';
renderTagCategId = 'JCP|Welcome';
previousPageId = '';
var isFromSearchTerm = '';
var xFThirteen = '';
var xFTwelve = '';
var cmContainerId = '';
if (jcp_t == "G1") {
var cmCStartChar = jcp_c.substr(0, 1);
if (cmCStartChar == 'D' || cmCStartChar == 'd') {
xFThirteen = 'GLOBALNAV';
} else {
xFThirteen = '';
}
xFTwelve = 'JCP|Welcome';
}
cmContainerId = '';
if (cmContainerId != '') {
xFThirteen = cmContainerId;
if(xFThirteen == 'JCP|Welcome') {
xFThirteen = '';
}
xFTwelve = 'JCP|Welcome';
}
var pageID = 'Welcome';
pageID = removeEncodedSpecialChar(pageID);
var userState = getCookie('DP_USER_STATE');
var cookiedProfileVid = getCookie('cmvid');
var cookiedOrderVid = getCookie('jcpvid');
var ordVid = '';
var customerID = '';
if(userState == '1'){
ordVid = cookiedProfileVid;
customerID = cookiedProfileVid;
} else {
if(cookiedProfileVid == '' || cookiedProfileVid == undefined || cookiedProfileVid == 'null' ){
ordVid = cookiedOrderVid;
} else {
ordVid = cookiedProfileVid;
}
}
if(ordVid == undefined){
ordVid = cookiedProfileVid;
}
// Tracking tag for Guest and Authenticated user
var trackGuestAuthUser = '';
var isExpressCheckout = '';
var typeOfCheckoutCm31 = "";
if (trackGuestAuthUser) {
if(userState == '1'){
if (isExpressCheckout == 'true') {
typeOfCheckoutCm31 = "authenticated customer_express checkout";
} else {
typeOfCheckoutCm31 = "authenticated customer_regular checkout";
}
} else {
typeOfCheckoutCm31 = "guest checkout";
}
}
var clientId = '90024642';
var renTagUrl = 'www88.jcpenney.com';
var renTagDomain = 'jcpenney.com';
var mmcParamArray = getMarketingAttributeFromCookie();
cmSetClientID(clientId,false,renTagUrl,renTagDomain);
cmSetupOther({"cm_FormPageID":true});
// Start changes for DPPROG-117
var cmRedirectTerm = '';
// End changes for DPPROG-117
var categId = 'JCP|Welcome';
var searchTerm = '';
var searchResults = '';
var grView = '';
/*
var rendTopDime = '';
var rendTopDimeVal = '';
var currDime = '';
var currDimeVal = '';
var dimeCombo = '';
var dimeComboVal= '';
*/
var pageNumber = '';
var sortType= '';
var cmjcpC ='';
var predictveSearch = '';
var rendrTagJcp = '';
var cmRe18 = '';
var cmRe19 = '';
var cmRe20 = '';
// New code added for DP-8091 - core metrics
var cmRe26 = '';
var slashSeperatedStr = "";
if (personalizedContentSlot != undefined && personalizedContentSlot != null && personalizedContentSlot != '' && personalizedContentSlot.length > 0){
slashSeperatedStr = personalizedContentSlot.join("|");
cmRe26 = slashSeperatedStr;
}
// End of DP-8091
var cmRe35= '';
if (isFromSearchTerm == "true"){
cmRe35="Top SEO Searches";
}
searchTerm = removeEncodedSpecialChar(searchTerm);
if (ordVid == "" && customerID != "") {
ordVid = customerID;
}
if (ordVid == 'null' || ordVid == undefined ) {
ordVid = "";
}
var rendTopDime = '';
var rendTopDimeVal = '';
var currDime = '';
var currDimeVal = '';
var dimeCombo = '';
var dimeComboVal = '';
var pageType = $.trim($('#page').text());
if(pageType == 'category' || pageType == 'searchResults') {
var dimOrder = getCookie("dimOrder");
if(dimOrder != null && typeof dimOrder != 'undefined' && dimOrder != '') {
var dimTabArray = dimOrder.split("+");
var currentTabDimensions = '';
var key = xgnCookieUtil.getkey();
for(var i = 0; i < dimTabArray.length; i++ ) {
if (typeof dimTabArray[i] != 'undefined' && $.trim(dimTabArray[i]) != "" ) {
var tempTabDimOrder = dimTabArray[i].split('~');
if(key == tempTabDimOrder[0]) {
var currentTabDimOrder = tempTabDimOrder[1];
currentTabDimensions = currentTabDimOrder.split(':');
break;
}
}
}
if( currentTabDimensions != null && typeof currentTabDimensions != 'undefined') {
var selectedDimensions = getValidDimensionIDs(currentTabDimensions);
var selectedDimensionsSize = selectedDimensions.length;
for(var i = 0; i < selectedDimensionsSize; i++) {
var currentDimId = selectedDimensions[i];
var currentDimName = removeEncodedSpecialChar($.trim($('#div'+currentDimId+' #currentDimName').val()));
var currentChildDimName = removeEncodedSpecialChar($.trim($('#ChildDimension'+ currentDimId +' ul li:first h4 span').text()));
var currentDimValueName = '';
if(typeof currentDimName != 'undefined' && currentDimName != null && currentDimName != '') {
var isExtDimension = $('#BBUL' + currentDimId + ' #isExtDimension' + currentDimId).val();
if(isExtDimension == 'true') {
currentDimValueName = removeEncodedSpecialChar($.trim($('#BBUL'+ currentDimId +' li:last').text()));
} else {
currentDimValueName = removeEncodedSpecialChar($.trim($('#BBUL'+ currentDimId +' li:first').text()));
}
} else if(typeof currentChildDimName != 'undefined' && currentChildDimName != null && currentChildDimName != '') {
currentDimName = currentChildDimName;
currentDimValueName = removeEncodedSpecialChar($.trim($('#ChildDimension'+ currentDimId +' ul .checkboxon a').text()));
}
if(currentDimNameOfMKL=='Marketing Label'){
currentDimName=currentDimNameOfMKL;
currentDimNameOfMKL="";
}
dimeCombo += (currentDimName + '|');
dimeComboVal += (currentDimValueName + '|');
if(i == 0) {
rendTopDime = currentDimName;
rendTopDimeVal = currentDimValueName;
} else if(i == selectedDimensionsSize - 1) {
currDime = currentDimName;
currDimeVal = currentDimValueName;
}
}
if(typeof currDime == 'undefined' ||  currDime == null ||  currDime == '') {
currDime = rendTopDime;
currDimeVal = rendTopDimeVal;
}
}
}
}
if(renderTagPageId == 'Timeout'){
var jcptag = jcp_t+'-_-'+jcp_c+'-_-'+'-_-'+ordVid+'-_-'+rendTopDime+
'-_-'+rendTopDimeVal+'-_-'+currDime+'-_-'+currDimeVal+
'-_-'+dimeCombo+'-_-'+dimeComboVal+'-_-'+pageNumber+'-_-'+sortType+'-_-'+'-_-'+
'-_-'+'-_-'+previousPageId+cmRedirectTerm+'-_-'+cmRe18+'-_-'+cmRe19+'-_-'+cmRe20+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+cmRe26+
'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+predictveSearch+'-_-'+cmRe35+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+mmcParamArray[0]+'-_-'+mmcParamArray[1]+'-_-'+mmcParamArray[2]+'-_-'+mmcParamArray[3];
} else {
var jcptag = jcp_t+'-_-'+jcp_c+'-_-'+'-_-'+ordVid+'-_-'+rendTopDime+
'-_-'+rendTopDimeVal+'-_-'+currDime+'-_-'+currDimeVal+
'-_-'+dimeCombo+'-_-'+dimeComboVal+'-_-'+pageNumber+'-_-'+sortType+'-_-'+'-_-'+
'-_-'+'-_-'+'-_-'+cmRedirectTerm+'-_-'+cmRe18+'-_-'+cmRe19+'-_-'+cmRe20+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+cmRe26+
'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+typeOfCheckoutCm31+'-_-'+'-_-'+'-_-'+predictveSearch+'-_-'+cmRe35+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+mmcParamArray[0]+'-_-'+mmcParamArray[1]+'-_-'+mmcParamArray[2]+'-_-'+mmcParamArray[3];
}
// var cmjcpTag = '-_-'+'-_-'+'-_-'+sortType+'-_-'+'-_-'+'-_-'+'-_-'+jcp_m+'-_-'+'-_-'+jcp_t+'-_-'+jcp_c+'-_-'+xFTwelve+'-_-'+xFThirteen+'-_-'+'-_-'+ordVid;
var valueP = document.getElementById("pageValue");
if(valueP != null || valueP != undefined)
{
document.getElementById("pageValue").value = categId;
}
// Start DP-10850
cmCreatePageviewTag(pageID,categId,searchTerm,searchResults,jcptag);
// End DP-10850
updateCommonTagDataLayerProperties(renderTagCategId,renderTagPageId,searchTerm);

//Start DPPROG-1423 setting coremetrics Host for POSF
//END DPPROG-1423 setting coremetrics Host for POSF
}
