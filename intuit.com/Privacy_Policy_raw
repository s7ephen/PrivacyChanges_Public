<script type="text/JavaScript">
var isMarketingEmailOptinEnabled = "false";
var applicationContextPath = "/dotcom";
var marketingEmailOptinCookieName = "MarketingEmailOptin";
var marketingEmailIdentfierParam = "cm_mmc"
var marketingEmailOptinCookieName = "MarketingEmailOptin";
var marketingEmailOptinCookieDomain = "jcpenney.com";
var marketingEmailOptedInCoookieMaxAge = "31536000000";
var marketingEmailOptedOutCoookieMaxAge = "15768000000";
var previousUrlCoookieMaxAge = "2592000";
var loadModalDelayTime = '6000';
var serverSecurePort = "443";
var serverName = "www.jcpenney.com";
var templateUrl = '';
// /(cm_mmc=e|cm_mmc=E)/i
var marketingEmailILinkdentfierRegExp = new RegExp('(' + marketingEmailIdentfierParam + '=e|' + marketingEmailIdentfierParam + '=E' + ')','i');
templateUrl = '/jsp/optin/secure/marketingEmailOptinOverlay.jsp';
$(document).ready(function(){
var isDisplayModal = displayOrNotDisplayMarketingOptinFlyout();
if(isDisplayModal) {
setTimeout(displayMarketingOptionModal, loadModalDelayTime);
//fnInitiateModal('https://' + serverName + ':' + serverSecurePort + applicationContextPath + templateUrl +'?parentUrl='+window.location.href);
// By default, after opening the overlay setting marketing email optin cookie status to OPTED_OUT
// If user opts in, cookie is updated with status OPTED_IN and with corresponding exipre time.
//setMarketingEmailOptinCookie('OPTED_OUT', marketingEmailOptedOutCoookieMaxAge);
}
});
function displayMarketingOptionModal() {
fnInitiateModal('https://' + serverName + ':' + serverSecurePort + applicationContextPath + templateUrl +'?parentUrl='+window.location.href);
// By default, after opening the overlay setting marketing email optin cookie status to OPTED_OUT
// If user opts in, cookie is updated with status OPTED_IN and with corresponding exipre time.
setMarketingEmailOptinCookie('OPTED_OUT', marketingEmailOptedOutCoookieMaxAge);
}
function displayOrNotDisplayMarketingOptinFlyout() {
// Don't display the overlay if not enabled.
if(isMarketingEmailOptinEnabled != 'true')
return false;
// Don't display the overlay if user is coming from marketing email,
// and set the cookie to OPTED_IN(User might have been opted in different browser).
var previousUrl = getCookie('previousUrlCookie');
var pageUrl = window.location.href;
setPreviousUrlCookie(pageUrl, previousUrlCoookieMaxAge);
var isMarketingEmailParamExists = pageUrl.search(marketingEmailILinkdentfierRegExp);
if(isMarketingEmailParamExists != -1) {
setMarketingEmailOptinCookie('OPTED_IN', marketingEmailOptedInCoookieMaxAge);
return false;
}
// Don't display the overlay if protocol is HTTPS.
var requestProtocol = window.location.protocol;
if(requestProtocol != 'http:')
return false;
// don't display the overlay if it's cart page
var checkoutUrl = isCheckoutUrl(pageUrl);
if (checkoutUrl) {
return false;
}
// Don't display the overlay for international customers.
var shipToCountry = getCookie('shipToCountry');
if(shipToCountry != undefined && shipToCountry != null && shipToCountry != '' && shipToCountry != 'US')
return false;
//don't display overlay if it is first page in jcp site or order already complete
if (previousUrl == null || previousUrl == '') {
return false;
}
//don't display overlay if order already complete
if (isOrderConfirmationPage(previousUrl)) {
setMarketingEmailOptinCookie('OPTED_OUT', marketingEmailOptedOutCoookieMaxAge);
return false;
}
// Display the overlay if marketingEmailOptinCookie cookie is not available/expired.
var marketingEmailOptinCookie = getCookie(marketingEmailOptinCookieName);
if(marketingEmailOptinCookie == undefined || marketingEmailOptinCookie == null || marketingEmailOptinCookie == '')
return true;
// Marketing cookie is available with status either "OPTED_IN" or "OPTED_OUT" so, don't display the overlay.
return false;
}
function setMarketingEmailOptinCookie(value, expireTimeMS) {
var date = new Date();
date.setTime(date.getTime() + parseInt(expireTimeMS));
var expires = "; expires=" + date.toGMTString();
document.cookie = marketingEmailOptinCookieName + "=" + value + expires + "; path=/;domain="+marketingEmailOptinCookieDomain;
}
function setPreviousUrlCookie(value, expireTimeMS) {
var date = new Date();
date.setTime(date.getTime() + parseInt(expireTimeMS));
var expires = "; expires=" + date.toGMTString();
document.cookie = "previousUrlCookie=" + value + expires + "; path=/;domain="+marketingEmailOptinCookieDomain;
}
function isCheckoutUrl(url) {
if (url.indexOf('/cart/') > -1)
{
return true;
}
return false;
}
function isOrderConfirmationPage(url) {
if (url.indexOf('orderConfirmation.jsp') > -1) {
return true;
}
return false;
}
</script>

