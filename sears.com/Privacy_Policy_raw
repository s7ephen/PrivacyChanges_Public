<script>
shc.doAdSense = function(pageId) {
	var p1, p2, pubId = shc.storeName;
	
	switch (pageId) {
		case 'pdp': pubId += '-pdp'; break;
		case 'search-page':
			pubId += (bt.pageType === 'search' ? '-search' : '-browse');
                        $('#gnf_footer_GADS').html('');
			break;
	}
	
	p1 = {
		pubId: pubId,
		adtest: shc.env === 'PROD' ? 'off' : 'on',
		query:	(pageId === 'product' ? bt.productName :
				pubId.indexOf('-search')>0 ? bt.keyWord :
					(bt.levels || bt.categoryName || bt.verticalName)).replaceAll('_',' '),
		channel: (bt.verticalName || bt.categoryName || 'misc').replace(/[\s&\s\,]/g, '_').replace(/_+/g, '_').toLowerCase(),
		adPage: $.query.load(location.search).get('pageNum') || 1
		};
	p2 =  {
		container: 'gnf_footer_GADS', width: '600px',
		colorLocation: '#333333', colorAnnotation: '#666666', colorAttribution: '#333333', colorBackground: '#FFFFFF',
		colorBorder: '#d0d0d0', colorDomainLink: '#333333', colorText: '#333333', colorTitleLink: '#333333',
		fontSizeAnnotation: 13, fontSizeDescription: 13, fontSizeDomainLink: 13, fontSizeTitle: 15, fontSizeLocation: 13,
		siteLinks: true, lineHeightTitle: 20, lineHeightDescription: 14, lineHeightDomainLink: 14, titleBold: true,
		verticalSpacing: 2, linkTarget: '_blank', longerHeadlines: true,
		number: 6, sellerRatings: true, detailedAttribution: true
	};
console.log('Adsense P1:');console.log(p1);
console.log('Adsense bt:');console.log(bt);
	new google.ads.search.Ads(p1, p2);
	$('#gnf_footer .GADS_wrap').show();
};

$(function() {
	$('#gnf_header, #gnf_footer').find('a').each(function(idx) {
		var el = $(this), href = el.attr('href');
		if (!!href && href.startsWith('{')) {
			href = href.replace('{profile}', shc.URLs.profile)
					.replace('{orderCenter}', shc.URLs.orderCenter)
					.replace('{searsCard}', shc.URLs.searsCard)
					.replace('{listCenter}', shc.URLs.listCenter)
					.replace('{returns}', shc.URLs.returns)
					.replace('{manageAccount}', shc.URLs.manageAccount);

			el.attr('href', href);
		}
	});
	
	var pageId = $.q('body').attr('id');
 
	
	//include Adsense only only on search/pdp
	if (['sears', 'kmart'].indexOf(shc.storeName) >= 0 && pageId === 'search-page' && $('#gnf_footer_GADS').length) {
		$(window).on('SearchContentChanged', function() {
			console.log('SearchContentChanged: Level=' + bt.levels);
   			if (!shc.AdSenseJS) {
			   FED.Util.addJsFile('//www.google.com/adsense/search/ads.js')
					.done(function() {
						shc.doAdSense(pageId);
						shc.AdSenseJS = true;
					});
			}
			else {
				shc.doAdSense(pageId);
			}
		});
	}

	//push Ad stuff further to allow PDP and other pages to render needed data
	$('#OAS_posBottom').remove(); //hack this out - still in ASAP template
	$('#gnf_footer').find('.OAS_wrap').show();

	//exclude hooklogic from vert/cat/BOF
	//if (['category', 'vertical', 'shoppingcart'].indexOf(pageId) < 0 && location.protocol === 'http:') {
	//	$('#gnf_footer .HL_wrap').show();
	//}
});

$(window).on('load', function() {
 if (!!$.cookie('irp-preview')){
  FED.Util.addJsFile(shc.URLs.staticAssets + 'js/previewHeader');
 }
/* pending UHF-405
 if ($('#profileArea.loggedIn').length) {
  $('#gnf_header, #gnf_footer').find('a[data-alt-href]').each(function() {
   var el = $(this), alt = el.attr('data-alt-href');
   if (!!alt) {el.attr('href', alt);}
  })
 }
*/
});
</script>




</div><script type="text/javascript">

	window.bt = {"levelIds":"",
	"levels":"",
	"verticalId":"",
	"verticalName":"",
	"categoryID":"",
	"categoryName":"",
	"subCategoryID":"",
	"subCategoryName":"",
	"itemNumber":"",
	"beanType":"",
	"itemPrice":"N/A",
	"itemDiscount":"N/A",
	"productDescription":"",
	"oasSitePage":"sears.com/customerservice/policies/privacypolicy/index/"};
	(function() {
		var catArray = shc.catArray;
		var catIdArray =  shc.catIdArray;
		if (catArray.length > 0) {
			var i = 0, l = Math.min(3, catArray.length);
			for (i = 0; i < l; i++) {
				switch (i) {
					case 0:
						bt.verticalName = catArray[i];
						bt.levels = bt.verticalName;
						bt.verticalId = catIdArray[i];
						bt.levelIds = bt.verticalId;
						break;
					case 1:
						bt.categoryName = catArray[i];
						bt.levels += '_'+bt.categoryName;
						bt.categoryID = catIdArray[i];
						if(bt.categoryID !== ""){
						bt.levelIds += '_'+bt.categoryID;
						}
						break;
					case 2:
						bt.subCategoryName = catArray[i];
						bt.levels += '_'+bt.subCategoryName;
						bt.subCategoryID = catIdArray[i];
						if(bt.subCategoryID !== ""){
						bt.levelIds += '_'+bt.subCategoryID;
						}
				}
			}
		}
	})();
</script>

