<script type="text/javascript">
//Common values which can be derived for all pages
shc = $.extend(shc, {
  commonQS: '?storeId=' + shc.storeId + '&catalogId=' + shc.catalogId + '&langId=' + shc.langId,
  env:/(qa\.ecom\.)|(dev\.ecom\.)/.test(location.hostname) ? 'QA' :
   /(pilot\.)/.test(location.hostname) ? 'PILOT' :
   /(beta\.)/.test(location.hostname) || $.query.get('origin').toUpperCase() === 'BETA' ? 'BETA' :
   /(staging\.)/.test(location.hostname) ? 'STAGE' : 'PROD',
  sslPort: /(qa\.ecom\.)|(dev\.ecom\.)/.test(location.hostname) ? location.port.substr(0,2) + '43' : '443',
  isKiosk: trim($.cookie('KI_FLAG')).toLowerCase() === 'true',
  casServerName: /(qa\.ecom\.)|(staging)/.test(location.hostname) ? 'http://ssoqa.shld.net' :
   /stress/.test(location.hostname) ? 'http://ssovip.stress.ch3.s.com:80' :
   /(dev\.ecom\.)/.test(location.hostname) ? 'http://web301p.dev.ch3.s.com:5780' :
   /(i\-prod\.ch)|(prestage\.ch)/.test(location.hostname) ? 'http://sso.shld.net' : 
   'http://sso.shld.net',
  casServerNameHttps: /(qa\.ecom\.)|(staging)/.test(location.hostname) ? 'https://ssoqa.shld.net' :
   /stress/.test(location.hostname) ? 'https://ssovip.stress.ch3.s.com:443' :
   /(dev\.ecom\.)/.test(location.hostname) ? 'https://web301p.dev.ch3.s.com:5743' :
   /(i\-prod\.ch)|(prestage\.ch)/.test(location.hostname) ? 'https://sso.shld.net:443' : 
   'https://sso.shld.net:443',
  isOutlet: /SHC\-HTS|SHC\-HAS|SHC\-AHS|SHC\-OUT/.test(navigator.userAgent),
  feedbackProvider: 'QUAL', // used by O_LC_Utility in shcGlobalTracking.js
  footer: false, 
  IRP: trim($.cookie('ot')).startsWith('i'),
  entCSSVersion: '.1.5.0'
});
shc.asapAkamaiBasePath = shc.asapAkamaiBasePath || '';
//need to have path prefixed for dynamic file adds in IE8
shc.imagePath = (shc.imagePath.indexOf('http') >=0 ? '' : location.protocol) + shc.imagePath;

shc.btSite = (function() {
    var map = {
     '10151':'IctsMUI', '10151_qa':'QqDbVSM', '10151_kk':'fvqvh1D',
     '10153':'PCNPFl9', '10153_qa':'for2rjl', '10153_kk':'QDsdc7e',
     '10154':'mrIBbYB', '10154_qa':'VyMh3te',
     '10155':'BciA6ZX', '10155_qa':'G9zuKqq',
     '10165':'PCNPFl9', '10165_qa':'PCNPFl9'
    };
    return (shc.env ==='PROD' ? map[shc.storeId + (shc.isKiosk ? '_kk' : '')] : map[shc.storeId + '_qa']);
}());

shc.URLs = $.extend(shc.URLs, {
    chronometer: shc.env === 'QA' ? '//navappvip.qa.ch3.s.com/' : shc.env === 'STAGE' ? '//navappvip.stress.ch3.s.com/' : '//nav.shld.net/',
    secureURL: String.format('https://{1}:{0}/', shc.sslPort, location.hostname),
    chronData: 'segments/',
    chronAssets: 'assets/',
    orderStatus: String.format('https://{1}:{0}/universalprofile/OrderHistory?upid={2}',shc.sslPort, location.hostname, shcWidgets.USR_SITEID[shc.storeId]),
    orderStatusError: '/shc/s/OrderStatusLoginModalView' + shc.commonQS + '&shcapiBypassSSO=true&errorCode=&OrdStatErr=',
    logonForm: '/shc/s/LogonForm' + shc.commonQS + '&URL=TopCategoriesDisplay',
    userLogoff: String.format('https://{1}:{0}/shc/s/Logoff{2}' +
    '&personalizedCatalog=true&LogoffFlag=true&URL=TopCategoriesDisplay&rememberMe=false', shc.sslPort, location.hostname, shc.commonQS),
    profile: String.format('https://{1}:{0}/universalprofile/dashboard?upid={2}', shc.sslPort, location.hostname, shcWidgets.USR_SITEID[shc.storeId]),
    orderCenter: String.format('https://{1}:{0}/universalprofile/checkorderstatus?upid={2}',shc.sslPort, location.hostname, shcWidgets.USR_SITEID[shc.storeId]),
   userLoginView: String.format('https://{1}:{0}/shc/s/USRLoginViewHttps{2}', shc.sslPort, location.hostname, shc.commonQS),
    scene7: '//s.shld.net/is/image/Sears/',
    listCenter: String.format('https://{1}:{0}/universalprofile/listcenter?upid={2}',shc.sslPort, location.hostname, shcWidgets.USR_SITEID[shc.storeId]),
    miniCartCount: '/crsp/api/cart/v1/itemcount',
    addToCart: shc.IRP ? '/crsp/mx/cart' : String.format('/shc/s/OrderItemDisplay{0}&orderId=.', shc.commonQS),
    staticAssets: '//c.shld.net/staticassets/Assets_0.90/',
    searsCard: String.format('https://{1}:{0}/universalprofile/ApplyCitiSearsCard?upid={2}',shc.sslPort, location.hostname, shcWidgets.USR_SITEID[shc.storeId]),
    returns: String.format('https://{1}:{0}/universalprofile/makeareturn?upid={2}',shc.sslPort, location.hostname, shcWidgets.USR_SITEID[shc.storeId])
});

shc.features = $.extend(shc.features, {
  autofill: ['10151', '10153', '10165'].indexOf(shc.storeId) >=0,
  omniture: true,
  SYWR: {enabled: true, pointsExpirationThreshold: 60, refresh: 5},
  regPricing: {enabled: 'ON'  /*legacy stupidity - should be a boolean*/},
  disableHdrMiniCart: true,
  newMiniCart: shc.storeId !== '10165',
  reserveIt: shc.storeId === '10153',
  sywrOffer: true,
  ssoDisabled: false, //SSOContingency from WCS
  headerCoupons: {
    enabled: ['10151', '10153'].indexOf(shc.storeId) >= 0,
      shcApi: {
      details: shc.env === 'QA' ?
       'https://web301p.qa.ch3.s.com:6243/shcapi/v2/userCoupon/json/getCouponInfo,10000,abcdefg%24%24%24%24%24%24%24%24123456782,UCoupon_BRW' :
       'https://webservices.sears.com/shcapi/v2/userCoupon/json/getCouponInfo,1500,ecpnbrwBA6DF77123C6A9A1A0AB5466AA5192013,UCoupon_BRW',
      headerClient: 'hdr',
      headerFields: 'count'
   }
  },
  chat247: shc.storeId==='10153' && location.port === '4780',
  load_sCode: false,
  fitment: false
});

// legacy global vars used by modalLogin.js/SPURefactor*.js
var storeId = shc.storeId, catalogId = shc.catalogId, langId = shc.langId,
