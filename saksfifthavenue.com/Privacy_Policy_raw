					<script type="text/javascript">
						(function($){
							var search_element = $('#site-search');
							var field = search_element.find('[name="SearchString"]');
							if ('placeholder' in field[0]) 
								field.val('');
							else 
								field.val('Search');

							$('[name="endeca_search_form_one"]', search_element).submit( function (evt){
								var _val = field.val();
								if ( _val.length === 0){
									field.val(' ');
								}
							});
						}(jQuery));
                    </script>

