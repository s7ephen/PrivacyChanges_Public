			'SaksBeautyPlace'       :'BF',
			'saksbeautyplace'		:'BF',
			'Beauty & Fragrance'    :'BF',
			'beauty & fragrance'    :'BF',
			'Men'                   :'MS',
			"Men's"                 :'MS',
			'themensstore'			:'MS',
			'TheMensStore'          :'MS',
			"The Men's Store"       :'MS',
			"the men's store"       :'MS',
			'Kids'                  :'JK',
			"Kid's"                 :'JK',
			'JustKids'              :'JK',
			'justkids'              :'JK',
			'Just Kids'             :'JK',
			'just kids'             :'JK',
			'Home'                  :'HG',
			'home & gourmet'        :'HG',
			'HomeHiTech'            :'HG',
			'homehitech'            :'HG',
			'Home & Gourmet'        :'HG',
			'Sale'                  :'SL',
			'SaksSale'              :'SL',
			'sakssale'              :'SL',
			'sale'                  :'SL',
			'GiftsHomeKids'         :'GS',
			'giftshomekids'			:'GS',
			'gift shop'             :'GS',
			'Gift Shop'             :'GS'
	    };

	    var _saksBody = $('#saksContainer');
	    var _mediator = Saks.module('Mediator');
	    var topNavTracking = function(_this){
			// set cookie as a flag to indicate
			// user clicked top nav
			var category_id = $(_this).closest('.menu-item').attr('id');
			category_id = category_id.replace('NavMenu', '');
			
			category_id = category_id.toLowerCase();
			

			//for story 03323
			//pass in the cateogry id where the user clicked to the prefix object
			//if there's a result, use it instead
			category_id = (_category_prefix[category_id]) ? _category_prefix[category_id] : category_id;
			createCookie('2888', category_id, 1);
		};

		var leftNavTracking = function (_this){
			var _label = (s.eVar59) ? s.eVar59 : s.pageName;
	        _label = _label.replace(/:(\d+)/g, '');
	        
	        //for story 03323
	        //split _label and get the string before :
	        //this should be the string for the category
	        _label = _label.split(':')[0];
	        
	        //for story 03323
	        //pass in the category to the prefix object 
	        //if there's a result, use it instead
	        _label = (_category_prefix[_label]) ? _category_prefix[_label] : _label;
	        createCookie('leftNavOmn', _label, 1);
		};

	    $('#header', _saksBody).on('click', '.nav a', function() {
	    	_mediator.publish('saks:tracking:topnav:links', this);
	    });

	    $('.sfa-left-nav', _saksBody).on('click', 'a', function() {
	    	_mediator.publish('saks:tracking:leftnav:links', this);
	    });

   		_mediator.subscribe("saks:tracking:topnav:links", topNavTracking, this);
		_mediator.subscribe("saks:tracking:leftnav:links", leftNavTracking, this);
	    
	    var cookie = getCookie('2888');
	    if (cookie) {
	    	_mediator.publish( "ANALYTICS:OMNITURE:ONLOAD", 'topnav', { cookie: cookie });
	    	eraseCookie('2888');
	    }

	    cookie = getCookie('leftNavOmn');
	    if ( cookie ) {
	    	_mediator.publish( "ANALYTICS:OMNITURE:ONLOAD", 'leftnav', { cookie: cookie });
	    	eraseCookie('leftNavOmn');
	    }

	

})(jQuery);

