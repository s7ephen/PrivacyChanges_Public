function EML1145A() {
	pageName = "class com.bluemartini._jsp._templates._html._popups.__legal";
	pageName = pageName.substr(39);

	var cameFromGoogle = self.location.href.indexOf("site_refer=360i+G");
	var cameFromGoogleAds = self.location.href.indexOf("site_refer=BLOCKER");
	var cameFromEML = self.location.href.indexOf("site_refer=EML");
	var daCookie = document.cookie;

	if ( (daCookie.indexOf("EML1145A=TRUE") == -1 && !intlNav) && (pageName != "checkout.__thankyou" || pageName != "checkout.__saksbag2" || pageName != "account.__account" || pageName != "account.__registration" || pageName != "html._email_popup.__email_collect_data" || pageName != "html._email_popup.__email_collect_popup_main" || pageName != "html._email_popup.__SaksFirst_Email_ThankYou")) {
		if (cameFromGoogle == -1 && cameFromEML == -1 && cameFromGoogleAds == -1) {

			// publish email collect emailCollect.js
			Saks.module('Mediator').publish( "EML:emlCollectInit" );
		}
	}

    createCookie("EML1145A", "TRUE", 365);
}

