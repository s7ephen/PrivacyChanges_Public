	R3_SEARCH.setTerms(s.eVar21);
	if(rrSearchItems.length > 0){
		for (x=0;x<rrSearchItems.length;x++){
			//alert("search "+rrSearchItems[x]);
			R3_SEARCH.addItemId(rrSearchItems[x]);
		}
	}
}
r3();
