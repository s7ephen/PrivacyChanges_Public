<script language="JavaScript" type="text/javascript">

var SaksBagShowing = 0; //Toggle to know when the overlay is showing
var AddCart = 0;
var num_items_added = 0;
var receiveReqOverlay;
var loadNewBag = 1;
var addedItemTimer;
var clickedOnDiv=null;
var ffResetFlag = {'global_reset': 'false'};
function getXmlHttpRequestObject() {
	if (window.XMLHttpRequest) {
		return new XMLHttpRequest(); //Not IE
	} else if(window.ActiveXObject) {
		return new ActiveXObject("Microsoft.XMLHTTP"); //IE
	} else {
		//alert("Your browser doesn't support AJAX.");
	}
}		
			
function loadBagNow(num_items_added){
	document.getElementById('saksbagoverlay_insert').innerHTML = "<table width='417' height='120'  class='BackDropColor' cellpadding='0' cellspacing='5' border='0'><tr><td align='center' valign='center' bgcolor='#ffffff'><img src='/media/images/saks_bag/loadingsaksbag_v1_m56577569831411277.gif' border='0'></td></tr></table>";
	var paramsOverlay = "";
	var urlOverlay = 'http://www.saksfifthavenue.com/include/saksbaginsert.jsp?bmUID=k_WS_at';
	var currentUrlnow = window.location.toString();
	var rehttp= new RegExp ("https://");
	if(currentUrlnow.match(rehttp)){
		if(urlOverlay.match(rehttp)){
			urlOverlay = urlOverlay;
		}else{
			urlOverlay = urlOverlay.replace(/http:/,"https:");
			//replace http with https
		}
	}

	var re= new RegExp (/\?/);
	if(urlOverlay.match(re)){
		paramsOverlay = "&num_of_items_added="+num_items_added;
	}else{
		paramsOverlay = "?num_of_items_added="+num_items_added;
	}
	urlOverlay += paramsOverlay;
	receiveReqOverlay.open("POST", urlOverlay, true);
	receiveReqOverlay.onreadystatechange = handleAddToBagOverlay;
	receiveReqOverlay.send(paramsOverlay);	
}
 
function handleAddToBagOverlay() {
	if (receiveReqOverlay.readyState == 4) {
		if(receiveReqOverlay.status  == 200){
			if(receiveReqOverlay.responseText){		
				document.getElementById('saksbagoverlay_insert').innerHTML = receiveReqOverlay.responseText;
				var qtyToAdd = getOmnFromInsert("omn_numItemsToAdd");
				omnAjaxCall(qtyToAdd);
			}
		}else{
			document.getElementById('saksbagoverlay_insert').innerHTML = "<table width='417' height='120'  class='BackDropColor' cellpadding='0' cellspacing='5' border='0'><tr><td align='center' valign='center' bgcolor='#ffffff'>Error: " + receiveReqOverlay.status +"<br>Could not load Saks Bag Overlay.<br>Page will Redirected to the Saks Bag Page</td></tr></table>";
			window.location.href="https://www.saksfifthavenue.com/checkout/SaksBag.jsp?bmUID=k_WS_au"
		}
	}
} 

function isIE6orLess(){
	if (isIE){
		var version=0;
		var temp=navigator.appVersion.split("MSIE");
		version=parseFloat(temp[1]);
		if(version < 7){
			return true;
		}else{
			return false;
		}
	}else{
		return false;
	}
}

			
function saksBagOpen(SaksBagOverlay,AddCart){

	if (SaksBagShowing || window.location.href.match(/(saksbag|checkout)\.jsp/i)) return;

	getNumberofItems();

	var $ = window.jQuery;

	var windowTopLocation = 0;
	var windowRightLocation = 0;	
	var currentTopLocation = $(window).scrollTop();
	var bodyWidth = $('body').innerWidth(), targetWidth, targetHeight, targetOffset;
	
	var $topnav = jQuery('#topNavContainer');
	var $toolbar = jQuery('#toolbar');

	if ($topnav.is(':visible')) {
		targetWidth = $topnav.find('.saksbag-nav-link').innerWidth();
		targetHeight = $topnav.find('#saks-nav-categories').innerHeight() + $topnav.find('#saks-nav-categories').offset().top;
		targetOffset = $topnav.find('.saksbag-nav-link').offset();
	} else if ($toolbar.is(':visible')) {
		targetWidth = $toolbar.find('.shopping-bag').innerWidth();
		targetHeight = $toolbar.innerHeight();
		targetOffset = $toolbar.find('.shopping-bag').offset();
	}

	if (currentTopLocation >= targetHeight) {
		windowTopLocation = currentTopLocation;
	} else {
		windowTopLocation = targetHeight;
	}

	windowRightLocation = bodyWidth - (targetOffset.left + targetWidth);
	
	// if(currentTopLocation >= 136){
	// 	windowTopLocation = currentTopLocation;
	// 	windowTopLocation = windowTopLocation - jQuery('#topNavContainer').innerHeight() - 5; 
	// }		

	if (Saks.config.PROMO_TOP_GLOBAL) windowTopLocation = windowTopLocation + 70;
	jQuery('#SaksBagOverlay').css({ 
		top: windowTopLocation, 
		right: windowRightLocation,
		zIndex: 9998
	});

	receiveReqOverlay = getXmlHttpRequestObject();

	if (AddCart >= 0){
		if (isIE6orLess()){
			HideAllPullDowns();
			showReplaceText('<br>');
		}
		num_items_added = AddCart;
		SaksBagShowing = 1;
		loadBagNow(num_items_added);
		setTimeout("jQuery('#SaksBagOverlay').slideDown(400)", 1000);
	} else {
		if (!(SaksBagShowing)){
			if (isIE6orLess()){
				HideAllPullDowns();
				showReplaceText('<br>');
			}
			SaksBagShowing = 1;
			loadNewBag = 1;
			setTimeout("jQuery('#SaksBagOverlay').slideDown(400)", 1000);
		}
		if (loadNewBag){
			loadBagNow("-1");
		}
	}
	setTimeout("activateCloseFromOutside()", 1000);

}

function overlayEffect (_param){
	jQuery('#' + _param).slideDown(400)
// 	Effect.SlideDown(_param, { duration: 0.4 });
}

function activateCloseFromOutside(){
	jQuery('#SaksBagOverlay').click(function() {
			clickedOnDiv = "";
	});
	jQuery(window.document).click(function (){
		if(clickedOnDiv==null){
			SaksBagClose();
		}
		clickedOnDiv=null;
	});
	//if(windowTopLocation > 0){
//		$('SaksBagOverlay').observe('click',function(event){
//			clickedOnDiv = "";
//		});
//		Event.observe(window.document, 'click', function() {

//		});
//}
}

function SaksBagClose(){
	if (ffResetFlag)
		ffResetFlag['global_reset']='true';
	if (SaksBagShowing){
		jQuery(window.document).unbind('click');
		clearTimeout(addedItemTimer);
		if (isIE6orLess()){
			ShowAllPullDowns();
			showReplaceText('');
		}
		SaksBagShowing = 0;
		jQuery('#SaksBagOverlay').slideUp(500, function (){
			jQuery('#SaksBagOverlay').css('top','0px');		
		});
// 		Effect.SlideUp(SaksBagOverlay, { duration: 0.5, afterFinish: resetSaksBagOverlay });
	}
}

// var resetSaksBagOverlay = function(){
// 	jQuery('#SaksBagOverlay').css('top','0px');
// }

onload = function addToBagOverlay(){
	var qtyAdded = '';
	if ((qtyAdded != null) && (qtyAdded != "") && (pageName.indexOf("checkout") < 0)){
		saksBagOpen('SaksBagOverlay',qtyAdded);
	}
}

function getOmnFromInsert(elementName){
	var elementNameReturned = "";
	var str = document.getElementById('saksbagoverlay_insert').innerHTML;
	var commentString = "\<\!-- "+elementName+"\=";
	var strLoc = str.indexOf(commentString);
	var str1 = str.substring(strLoc+(commentString.length));
	elementNameReturned = str1.substring(0, str1.indexOf(" --\>"));
	return elementNameReturned;
}

    //FIXME: Called only if Saks.config.PDP_FROM_PDS is false, and PDP_FROM_PDS is now in production
    function omnAjaxCallOld( addedItems ) {
	if((addedItems != null) && (addedItems != "-1")){
		s.events = "scAdd,event5,event6";
		s.prop1 = "add to bag";
		var firstTimeAddEvent = getOmnFromInsert( "omn_firstTimeEvent" );
		if( (firstTimeAddEvent != null) && (firstTimeAddEvent != "") ){
    		s.events = firstTimeAddEvent;
    	}
    	addedItemTimer = setTimeout("SaksBagClose('SaksBagOverlay')", 15000);
    }
	else{
    	s.events = "scView";
    	s.prop1 = "view bag";
    }

    s.products=getOmnFromInsert("omn_prodinfoStuff");
    //s.eVar6=getOmnFromInsert("omn_prodinfoSuggest"); // set in product string
    s.pageName="checkout:saks bag overlay";
    s.channel="checkout";
//     setTimeout("overlayEffect('SaksBagOverlay')", 1000);
	s.eVar53 = "+1";

	reserveController('init');

}

function omnAjaxCallNew( addedItems ) {
	logger.log( "omnAjaxCallNew" );
	if( !addedItems || addedItems === "-1" ) {
        s.events = "scOpen, scView";
    	s.prop1 = "view bag";
    	s.products=getOmnFromInsert("omn_prodinfoStuff");
    	s.pageName="checkout:saks bag overlay";
    	s.channel="checkout";
    	s.eVar53 = "+1";
	
	}
	reserveController('init');
}

//toggle for new PDS code
    //FIXME: PDP_FROM_PDS is now in production
function omnAjaxCall( addedItems ){

	if( Saks.config && Saks.config.PDP_FROM_PDS ) {
		omnAjaxCallNew( addedItems );
	} else {
		omnAjaxCallOld( addedItems );
	}
}

function reserveController(_type, _id, _this){
	if (_type == 'reserve'){
		overlayReserveService.requestPage (_id, overlayReserveSuccess.onSuccess)
	}
	else if (_type =='init'){
		ffResetFlag['global_reset'] = 'false';
		jQuery.each(jQuery('.timeRemain',  '.timer'), function (num1, it1){
			if (!ffResetFlag[jQuery(this).parent().attr('id').substr(5)])
				ffResetFlag[jQuery(this).parent().attr('id').substr(5)] = ({
					'timeR': jQuery(this).html()
				})
		});
		startTimer(_type);
	}
	else
		startTimer(_id, _type, _this);
}


function startTimer (flag, _this, _time) {
	var createEventTimer = function(selector, timeR, timesUpHandler, timeUpdatedHandler) {
		var timer;
	        	
	    var updateCountdown = function() {
	    	if (ffResetFlag['global_reset'] == "true"){
	    		clearInterval(timer);
	    	}
	    	else
	    		ffResetFlag['global_reset'] = 'false';
	    	var minutes = Math.floor(timeR / 60);
	    	var seconds = Math.floor(timeR % 60);
	    	var timerSelector = jQuery(selector + " .timer"); 
	    	var minSelector = jQuery(selector + " .minutes");
	    	var secSelector = jQuery(selector + " .seconds"); 
	    	var resSelector = jQuery(selector + " .reserveLink"); 
			jQuery(selector + " .minutes").text(minutes > 9 ? ("Reserved for "+ minutes + ":") : "Reserved for 0" + minutes + ":");
			jQuery(selector + " .seconds").text(seconds > 9 ? (seconds + " min ") : ("0" + seconds + " min"));
			jQuery(selector).css("display", "block");

			if ( timeR < 121 && timeR >0 ){
				timerSelector.css({"display" : "block",	"text-align":"left" , "font-weight" : "bold", "color" : "#990000"});
				resSelector.css('display','none');
				minSelector.css("color", "#990000");
				secSelector.css("color", "#990000");
				timeR--;
			}
			else if ( timeR> 0 ){
				timeR--;
				timerSelector.css({"display" : "block",	"text-align":"left" , "font-weight" : "bold", "color" : "#000000"});
				minSelector.css("color", "#000000");
				secSelector.css("color", "#000000");
				resSelector.css('display','none');
			}
			else
				timerSelector.css("display", "none");
				
			if ( timeR < 0 ){
				resSelector.css('display','none');
			}
			else if (timeR < 1){
				clearInterval(timer);
				timerSelector.css('display','none');
				resSelector.css('display','block');
			}
    
		};

	    updateCountdown();
	    timer = setInterval(updateCountdown, 1000);
	};
	
	if (flag == 'init'){
		jQuery.each(jQuery('.timer', '#saksbagoverlay_insert'), function (num1, it1){
			if (jQuery(it1).find('.minutes').length < 1 ){
				jQuery(it1).append('<div id="overlay' + jQuery(it1).attr('id').substr(5) + '" style="    display: block;   " ><div class="timer" style="color:#000; font-family: Helvetica, Arial, sans-serif;   font-size: 11px; "><span class="minutes"></span><span class="seconds"></span></div><div class="reserve"><span class="reserveLink" style="font-weight: bold; display:none; color: #990000;">No longer reserved<br /><a style="color: #990000;font-weight: bold;" href="#" onClick="reserveController(\'reserve\', '+ jQuery(it1).attr('id').substr(5) + ', this); 	return false;">Reserve Again</a></span><span style="display:none;"class="reserveFail">Sorry this item is no longer availalbe online. It will be removed from your Saks Bag upon checkout.<span></div>');
			}
 			createEventTimer("#overlay"+ jQuery(it1).attr('id').substr(5), jQuery(it1).find('span').html());
		});
	}
	else{
		createEventTimer("#overlay"+ flag, _time);
		jQuery(_this).parent().hide();
	}
}

var overlayReserveAjax = (function(){
	var _defaultErrorHandler = function(response){
		console.log('--- DEFAULT ERROR HANDLER ---')
		console.log(response);
	};
	
	var _saksAjax = function(_data, _successHandler, _reqURL, _reqType, _dataType, _errorHandler) {
		
		var _errorHandler = (_errorHandler) ? _errorHandler : _defaultErrorHandler;
		
		jQuery.ajax({
            url: _reqURL,
            type: _reqType,
            dataType: _dataType,
            data: _data,
            complete: _successHandler,
            error: _errorHandler
        });
    };
    
    return {
    	ajaxRequest : _saksAjax
    }
})();

var overlayReserveService = (function($){
	var _reqURL = '/checkout/checkout.jsp';
	var _reqType = 'POST';
	var _dataType = 'json';
	var _params = {bmForm : 'fashionfix_reserve_again_service'};
    
    return {
		requestPage : function(cartItemId, _successHandler){
				var data = $.extend({}, _params);				
				data.cartItemId = cartItemId;
    			overlayReserveAjax.ajaxRequest(data, _successHandler, _reqURL, _reqType, _dataType);
    	} 	
    };	
})(jQuery);

var overlayReserveSuccess = (function($){
	return {
		onSuccess: function (_result){
			s.eVar53 = "+1";
		
			var result = $.parseJSON(_result.responseText);
			var _currentID = this.data.substr (this.data.search('cartItemId=') + 11 );
			var cartItem = result.cartItems;
			$('#status'+_currentID).parents('tr').last().find('.whiteCheckOut').text('Estimated Total: $'+result.cartSummary.itemsTotal.substring(6));	
			$.each(cartItem, function (num1, it1){
				if (!ffResetFlag[it1.cartItemId]){
					ffResetFlag[it1.cartItemId] = ({
						'timeR' : it1.fashionFixTimer
					});
				}
				var gParent = $('#status'+_currentID).parent().parent().parent();
				if (it1.cartItemId == _currentID){
					var _currentQuantity = $('#overlay'+_currentID).parents('tr').first().find('.overlayQuantity').html();
					if (it1.quantity != _currentQuantity && it1.inventoryStatus == 'In Stock'){
						if (gParent.next().attr('class') !="overlayError"){
							var ffErrorMessage = '<tr class="overlayError"><td width="5"></td><td width="5"></td><td width="100%" valign="top" align="left" style="padding:5px 0px 5px 10px;" class="errors11" colspan="3">We are unable to reserve the full quantity that you have requested.</td><td width="5"></td></tr>';
							$(ffErrorMessage).insertAfter(gParent);
							gParent.find('.overlayQuantity').text(it1.quantity);
						}
							
					}
					else if (it1.inventoryStatus == 'Sold Out'){
						if (gParent.next().attr('class') !="overlayError"){
							var ffErrorMessage = '<tr class="overlayError"><td width="5"></td><td width="5"></td><td width="100%" valign="top" align="left" style="padding:5px 0px 5px 10px; " class="errors11" colspan="3">Sorry, this item is no longer available online. It will be removed from your Saks Bag upon checkout.</td><td width="5"></td></tr>';
							$(ffErrorMessage).insertAfter(gParent);
						}
					}
					else if (it1.inventoryStatus == 'In Member\'s Bag'){
						gParent.find('span.saksBagItemDetails').text(it1.inventoryStatus);
						gParent.find('.timer').first().css('display','none');
						if (gParent.next().attr('class') !="overlayError"){
							var ffErrorMessage = '<tr class="overlayError"><td width="5"></td><td width="5"></td><td width="100%" valign="top" align="left" style="padding:5px 0px 5px 10px; " class="errors11" colspan="3">Sorry, this item is no longer available online. It will be removed from your Saks Bag upon checkout.</td><td width="5"></td></tr>';
							$(ffErrorMessage).insertAfter(gParent);
						}
					}
					reserveController('ajax', _currentID, it1.fashionFixTimer);
					return false;
				}
				
			    var _itemLevelWarning = function(_arr) {
			        var a = [];
			        for (var i=0; i<_arr.length; i++) {
			            a.push('<div class="item-level-error"><div class="');
			            a.push(_arr[i].type);
			            a.push('">');
			            a.push(_arr[i].message);
			            a.push('</div></div>');
			        }
			        return a.join('');
			    };
			});				
		}
		
	};

})(jQuery);

</script>

