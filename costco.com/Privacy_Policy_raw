<script>
richRelevanceCarouselArray = [];
function carouselCallback(){
	//clean up structure of RR returned data
	var x = $("[id^='rr_placement_']").children(); 
	$("div.carousel ul").empty();
	$("div.carousel ul").append(x);
	
	//Perform carousel setup here for Rich Relevance
		try{
			//for each associated espot 
			// call the carousel with the parms associated to it 
			var arrayLength = richRelevanceCarouselArray.length;
			for (var i = 0; i < arrayLength; i++){
				var rrCarouselData = richRelevanceCarouselArray.pop();
				rrCarouselData.itemWidth = 185;
				$('#' + rrCarouselData.rrAssociatedESpot + ' .carousel').carousel(rrCarouselData);
			}
			
		}catch(err){
			console.log("RichRelevanceTopInclude error: " + err);
  		}
	};
</script>


