	<script charset="utf-8" type="text/javascript">	
	
		var richRelevance = {
				
				init: function(){
			 
					richRelevance.variables = {
							apiKey: 'dbeab3c977a08905',
							environment: "recs",
							userSessionID: "RI3qK86tV9sak80HFlQRO9wQKtomCUqDEh4=",
							userID: "RI3qK86tV9sak80HFlQRO9wQKtomCUqDEh4=" // if exists, set it; if not, set it as session ID
					};

					var rr = richRelevance.variables;
					
					if(typeof r3_common === 'function' && typeof R3_COMMON === 'undefined'){	
						window.R3_COMMON = new r3_common();			
						 R3_COMMON.setApiKey(rr.apiKey); 
						 R3_COMMON.setBaseUrl(window.location.protocol+'//' + rr.environment + '.richrelevance.com/rrserver/'); 
						 R3_COMMON.setClickthruServer(window.location.protocol+ '//' + window.location.host); 
						 R3_COMMON.setSessionId(rr.userSessionID); 
						 R3_COMMON.setUserId(rr.userID);
					}
					return R3_COMMON;
					
				},
				
				homepageRichRecs: function(){
					R3_COMMON = richRelevance.init();
					
					R3_COMMON.addPlacementType('home_page.product_slot_0'); 
					R3_COMMON.addPlacementType('home_page.featured_item_0'); 
					R3_COMMON.addPlacementType('home_page.featured_item_1'); 
					R3_COMMON.addPlacementType('home_page.featured_item_2'); 
					R3_COMMON.addPlacementType('home_page.featured_item_3'); 
					
					window.R3_HOME = new r3_home(); 
					
					r3();
				},
				
				product: function(pid, categoryID, placementArr){					
					R3_COMMON = richRelevance.init();					
					query_string = 'cid=PRIVACY_POLICY';
					findcgid = query_string.split('cgid=')[1] || query_string.split('q=')[1] || '';			
					prev_category = findcgid.split('&')[0];
					
					if(prev_category !== ''){
						R3_COMMON.addContext({ "userAttribute":{ "browse_path": prev_category }});
					}
					
					
						R3_COMMON.addPlacementType('item_page.rr1');						
						for(var i=0; i<placementArr.length; i++){				
							R3_COMMON.addPlacementType(placementArr[i]);							
						}		
										
						
				
					//If available, we highly recommend you provide the current relevant category ID 
					R3_COMMON.addCategoryHintId(categoryID); 
					window.R3_ITEM = new r3_item(); 
					window.R3_ITEM.setId(pid);	
					
					if(typeof R3_COMMON != 'undefined'){
						rr_flush_onload();
					}
					
					r3();
				},
				
				order: function(){
					R3_COMMON = richRelevance.init();

				/*	
					Placements Post Launch
					R3_COMMON.addPlacementType('purchase_complete_page.featured_item_0'); 
					R3_COMMON.addPlacementType('purchase_complete_page.featured_item_1'); 
					R3_COMMON.addPlacementType('purchase_complete_page.featured_item_2'); 
					R3_COMMON.addPlacementType('purchase_complete_page.featured_item_3'); 
				*/
					window.R3_PURCHASED = new r3_purchased(); 
					
				},
				
				category: function(categoryName, categoryID, placementNames){
					//check if search result or browse page
					
						R3_COMMON = richRelevance.init();									
						var placementArr = placementNames.split(",");
						
						for(var i=0; i<placementArr.length; i++){				
							R3_COMMON.addPlacementType(placementArr[i]);							
						}
						
						var R3_CATEGORY = new r3_category();					 
						R3_CATEGORY.setId('women'); 
						R3_CATEGORY.setName('women');					
						r3();
				
				},
				search: function(terms){
					R3_COMMON = richRelevance.init();
					
					R3_COMMON.addPlacementType('search_page.rr1_noresults');	
					
					var R3_SEARCH = new r3_search(); 
					R3_SEARCH.setTerms(terms);
					
					r3();
					
					rr_flush_onload();
					
					$(window).load(function() {
						setSlick();
					});
					
							function setSlick() {
								var initCount = $('#wrapper').hasClass('warehouse') ? 6 : 5;
									 $('.carouselInner ul').slick({
										 dots: false,
											infinite: true,
											speed: 600,
											arrows: true,
											slide: 'li',
											slidesToShow: initCount,
											slidesToScroll: initCount,
											onInit: function() {
												$(window).trigger('resize');
											},
											responsive: [
											    {
											    	breakpoint: 1200,
											    	settings: {
											    	slide: 'li',
											    	slidesToShow: 4,
											    	slidesToScroll: 4
											    	}
											    },
											    {
											    	breakpoint: 1024,
											    	settings: {
											    	  slide: 'li',
											    	  slidesToShow: 3,
											    	  slidesToScroll: 3
											    	}
											    },
											    {
											    	breakpoint: 767,
											    	settings: {
											    	  slide: 'li',
											    	  slidesToShow: 2,
											    	  slidesToScroll: 2
													}
												}
											]
									    });
								$(window).trigger('resize');
								}
							},
				
				homepageRichRecsLayout: function(){
					
					$('div[style="width:310px;height:310px;"]').css({ width: '100%', height: 'auto' });
					$.each($('.rrRecs div[id*="_container"]'), function(i, v){ 
						$(v).removeAttr('style').addClass('col-md-6 col-sm-12 col-xs-6');						
						$(v).find('img').removeAttr('width').addClass('img-responsive');
						//BSR-374
						var maxH = 0;
						var itemH = $(this).height();
						if(itemH > maxH) {
							maxH = itemH;
						}
						$('.rrRecs div[id*="_container"]').height(maxH);
					});
					$($('.rrRecs div[id*="_container"]').get(2)).addClass('hidden-xs hidden-sm');
					$($('.rrRecs div[id*="_container"]').get(3)).addClass('hidden-xs hidden-sm');
					// syncHeight doesn't work $($('.rrRecs div[id*="_container"]').syncHeight().delay(1500);
					
				},
				
				myDesignersNewArrivals: function() {
					R3_COMMON = richRelevance.init();
					R3_COMMON.addPlacementType('personal_page.rr1');
					window.R3_PERSONAL = new r3_personal(); 
					r3();
					$(window).load(function() {
						if(typeof rr_recs != 'undefined') {
							$('#personal_page_rr1').html( '<h2>Recommended</h2>' + rr_recs.placements[0].html );
							 
							//slickNewArr();
							 
						}
					});
					
					rr_flush_onload();
				}
				
		};	
		
	</script>	 
	    
		
	
	
	 


	



