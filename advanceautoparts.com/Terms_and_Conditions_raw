"undefined" ? a2cProductThumb : "";
typeof utag != "undefined" && utag.view({
page_type: "ajax_addtocart",
resx_itemid: c
});
if (a2cProductThumb=="/wcsstore/CVWEB/staticproductimage/images/NoImage_75x75.gif") a2cProductThumb = "/wcsstore/CVB2BDirectStorefrontAssetStore/images/common/blank.gif";
if (j("#a2cProductImg").length > 0) j("#a2cProductImg")[0].src = a2cProductThumb;
else j("#a2cOkTitle").after('<img id="a2cProductImg" width="100" height="100" src="' + a2cProductThumb + '" alt="' + j("#a2cCatName").html().replace(/"/g, "&quot;") + '" onerror="this.src=\'/wcsstore/CVWEB/staticproductimage/images/NoImage_75x75.gif\';">');
j("#a2cCatName").next().hasClass("clear") || j("#a2cCatName").after('<div class="clear">&nbsp;</div>');
if (a.notBuyable == "true") {
j("#a2cBackground").css({
opacity: "0.6"
}).fadeIn("slow");
j("#a2cOkNotBuyable").fadeIn("slow")
} else j.fancybox({
content: j("#a2cOk"),
autoSize: true,
autoResize: true,
padding: 0,
scrolling: "no",
openEffect: "none"
})
}
}
</script>
