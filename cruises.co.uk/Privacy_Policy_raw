<script type='text/javascript'>
var topNav = {
    init: function() {
        var mainNav = $('#topNav');
        navWidth = mainNav.width();
        navHeight = mainNav.height();
        // Some mobile support - tapping/clicking the menu item will open the menu, rather than opening the link
        $('#topNav .topNavItem>a').dblclick(function() {
            return true;
        });
        $('#topNav .topNavItem>a').click(function() {
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|Windows Phone|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                if (event.preventDefault)
                    event.preventDefault();
                event.returnValue = false;
                return false;
            } else {
                return true;
            }
        });
        mainNav.children('.topNavItem').hover(function(ev) {

//			 //show the dropdown

            $(this).addClass('topNavItemActive');
            //$('.youTubePlayer').hide();

        }, function(ev) {
            // hide the dropdown
            $(this).removeClass('topNavItemActive');
            //$('.youTubePlayer').show();
        });
    }
};
var navigationHeight = {
    fixHeight: function() {
        /* This insures that the left column and right column remain the same size */
        $('li.topNavItem > div').each(function() {
            var mainElementId = $(this).attr('id');
            var leftPanelHeight = $('#' + mainElementId + ' > .quickLinksPanel').height();

            if ((!leftPanelHeight)) {//If it is zero, try min-height
                leftPanelHeight = parseInt($('#' + mainElementId + ' > .quickLinksPanel').css('min-height'), 10);
            }
            //Get the right colum and make its with the same
            $('#' + mainElementId + ' > .mainPagesPanel').height(leftPanelHeight);
        });
    }
};
$(document).ready(function() {
    topNav.init();
    navigationHeight.fixHeight();
    $('li.topNavItem').click(function() {
        navigationHeight.fixHeight();
    });
});
$('li.topNavItem').click(function() {
    alert('trigger');

});
</script>
