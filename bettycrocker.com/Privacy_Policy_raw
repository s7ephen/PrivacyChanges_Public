<script type="text/javascript">
    jQuery('.blueLink').click(function () {       
        if (jQuery(this).attr('href').indexOf("www.facebook.com") > -1) {
            GeneralMills.Analytics.raiseEvent({ EventType: 'SocialFollow', SocialNetwork: 'Facebook' });
        }
        else if (jQuery(this).attr('href').indexOf("www.pinterest.com") > -1) 
        {
            GeneralMills.Analytics.raiseEvent({ EventType: 'SocialFollow', SocialNetwork: 'Pinterest' });
        }
        else if (jQuery(this).attr('href').indexOf("www.twitter.com") > -1)
        {
            GeneralMills.Analytics.raiseEvent({ EventType: 'SocialFollow', SocialNetwork: 'Twitter' });
        }
        else if (jQuery(this).attr('href').indexOf("www.youtube.com") > -1)
        {
            GeneralMills.Analytics.raiseEvent({ EventType: 'SocialFollow', SocialNetwork: 'YouTube' });
        }
        else if (jQuery(this).attr('href').indexOf("instagram.com") > -1)
        {
            GeneralMills.Analytics.raiseEvent({ EventType: 'SocialFollow', SocialNetwork: 'Instagram' });
        }
    });
</script>
