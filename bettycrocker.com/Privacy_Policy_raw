<script>
jQuery(function() {
jQuery( "#InitNav" ).click(function(){
    if ( jQuery( "#InitNav" ).hasClass('close-nav') ) {
        jQuery( "#InitNav" ).removeClass( "close-nav" ).addClass( "open-nav" );

    } else {
        jQuery( "#InitNav").removeClass( "open-nav" ).addClass( "close-nav" );
    } 
});
});

</script>
