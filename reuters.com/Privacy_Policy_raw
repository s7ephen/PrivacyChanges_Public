		<script type="text/javascript">
			function toggler(){
			  if ( $('#global-header-menu-discover').css('display') != 'block' ) {
				  $('#global-header-menu-discover').slideDown( 800, function() {
					  $( this ).css('display','block');});
				  $( '#chev' ).toggleClass( 'toggled' );
				  prepareLink();
			  } else {
				  $('#global-header-menu-discover').slideUp( 800, function() {
					  $( this ).css('display','');});
				  $( '#chev' ).toggleClass( 'toggled' );
			  }
			}
			
			function prepareLink(){
				d = new Date();
				var yyyy = d.getFullYear().toString();
				var mm = (d.getMonth()+1).toString(); // getMonth() is zero-based
				var dd  = d.getDate().toString();
				var currentDate = yyyy +"-"+ (mm[1]?mm:"0"+mm[0]) +"-"+ (dd[1]?dd:"0"+dd[0]);
				var str = "Events";
				var result = str.link("http://search.thomsonreuters.com/search?access=p&amp;output=xml_no_dtd&amp;sort=date:A:S:d1&amp;oe=UTF-8&amp;ie=UTF-8&amp;ud=1&amp;exclude_apps=1&amp;site=evo_prod_everything&amp;entqr=3&amp;entqrm=0&amp;q=inmeta:type%3Devent+inmeta:enddate:"+ currentDate +"..&amp;getfields=*&amp;client=evo_calendar&amp;proxystylesheet=evo_calendar&amp;filter=p");
				document.getElementById("demo").innerHTML = result; 
			}
		</script>	
