<script type="text/javascript">
/*<![CDATA[*/
           
    var browser=navigator.appName;
    
	$(document).ready(function(){
		
		//Search Page
		var sortOptions = $('#sortOptions');
		var pageSize = $('#pageSize');
			
		if (sortOptions != null){
			$('#sortOptions').attr('disabled','disabled');
		}
			
		
		if (pageSize != null){
			$('#pageSize').attr('disabled','disabled');	
		}

		prepareFindYourRepPopupFancy();
		//one more - findYourRepResultPopupFancy at startPage.tag
		
		$(".popupFancy").fancybox({
			'overlayOpacity' : 0.8,
			'overlayColor' : '#FFF',
			'centerOnScroll' : true,
			'autoScale':true,
			'autoDimensions':true,
			'scrolling':'no',
			'fitToView': true
		});
		
		$.fancybox.showActivity = function() {
			var fancyLoading = $("#fancybox-loading");
			fancyLoading.show();
	
		};
		
		$.fancybox.hideActivity = function() {
			var fancyLoading = $("#fancybox-loading");
			fancyLoading.hide();
		};
		
		$(".priceItPopupFancy").fancybox({
			'overlayOpacity' : 0.8,
			'overlayColor' : '#FFF',
			'centerOnScroll' : true,
			'autoScale':true,
			'autoDimensions':true,
			'scrolling':'no',
			'fitToView': true
		});
		
		$(".popupFancySimple").fancybox({
			'overlayOpacity' : 0.8,
			'overlayColor' : '#FFF',
			'centerOnScroll' : true
		});
		
		$(".popupFancyScroll").fancybox({
			'overlayOpacity' : 0.8,
			'overlayColor' : '#FFF',
			'centerOnScroll' : true,
			'scrolling':'yes'
		});
		
		$(".popupFancyWithProview").fancybox({
			'width' : 820,
			'height' : 480,
			'overlayOpacity' : 0.8,
			'overlayColor' : '#FFF',
			'autoScale':false,
			'autoDimensions':false,
			'scrolling':'no',
			'fitToView': true,
			'onStart':function(){
				$("#fancybox-wrap").css("visibility", "hidden");
			},
			'onComplete': function(){
				$("#fancybox-inner").children(":first").css("width","820px");
				$.fancybox.resize();
				$("#fancybox-wrap").css("visibility", "visible");
			},
			'onClosed':function(){
				$("#fancybox-wrap").css("visibility", "hidden");
			}
		});

		$(".gridTR").gridTR({
			
		});
		
		$.fn.gridTR.setting_default.url = '/law-products/p/getVolumes.json';
		
		$(".selectVolume").fancybox({
			 'width' : 830,
			 'height' : 480,
			 'overlayOpacity' : 0.8,
			 'overlayColor' : '#FFF',
			 'autoScale':false,
			 'autoDimensions':false,
			 'fitToView': true,
			 'scrolling':'no',
			 'onComplete':function(){
				 $.fn.gridTR.initLoad('');
				 $("#fancybox-wrap").css("visibility", "visible");
			 },
			 'onClosed':function(){
				 $.fn.gridTR.setting_default.productCode = '';
				 $.fn.gridTR.setting_default.sort = '';
				 $.fn.gridTR.setting_default.sortAtribute = '';
				 $.fn.gridTR.setting_default.date = '';
				 $("#fancybox-wrap").css("visibility", "hidden");
			 }			 
		});
		
		$(".modal").colorbox();

		$("div#menu").menuView();
		    	
	});
	
	//used in priceIt
	function prepareFindYourRepPopupFancy(){
		$(".findYourRepPopupFancy").fancybox({
			'overlayOpacity' : 0.8,
			'overlayColor' : '#FFF',
			'centerOnScroll' : true,
			'autoScale':true,
			'autoDimensions':true,
			'scrolling':'no',
			'onComplete': function() { tr.ecom.analytics.modalLoaded();}
		});
	}

	$(window).bind("load", function() {
		var slider = $("div#homepage_slider");
		if (slider != null){
			$("div#homepage_slider").slideView({easeTime: 400});	
		}
		
		//Search Page
		var sortOptions = $('#sortOptions');
		var pageSize = $('#pageSize');
			
		if (sortOptions != null){
			$('#sortOptions').attr('disabled','');
		}
			
		
		if (pageSize != null){
			$('#pageSize').attr('disabled','');	
		}
				
	});
	
/*]]>*/
 </script>
 
