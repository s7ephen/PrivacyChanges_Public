<script>
$(document).ready(function(){
	if(document.URL.match("/law-books")||document.URL.match("/publication-types")){
		$(".navlink div.lawbooks a").addClass("navselected");
	}
	else if(document.URL.match("/solutions")||document.URL.match("/software")||document.URL.match("/westlaw-legal-research")){
		$(".navlink div.solutions a").addClass("navselected");
	}
	else if(document.URL.match("/support")||document.URL.match("/product-support")){
		$(".navlink div.support a").addClass("navselected");
	}	
	else if(document.URL.match("/news-views")){
		$(".navlink div.newsviews a").addClass("navselected");
	}
	else{
		//do nothing
	}
	
	  $("#searchfield").focusin(function(){
	    $("#msg").hide();
	    $("#searchGoButton").css("height", "32");
	    var element = document.getElementById('searchGoButton');
	    element.style.background = '#ff8000';
	    element.style.border = "0px";
	    document.getElementById('searchGoButton').src="/law-products/_ui/swc/images/search-white_ICN.png";
	    
	    $("#searchfield").css("border", "1px solid #ff8000");
	    $("#searchfield").css("width", "370px");
	    $("#searchfield").css("color","#444444");	
	    $("#searchfield").css("text-align","left");
	   });
	 
	  $("#searchfield").focusout(function() {
		    $( "#msg" ).slideUp( 300 ).delay( 100 ).fadeIn( 400 );
			$("#searchGoButton").css("height", "32");
			var element = document.getElementById('searchGoButton');
		    element.style.background = '#ffffff';
		    element.style.borderTop = "1px solid #cccccc";
		    element.style.borderBottom = "1px solid #cccccc";
		    element.style.borderRight = "1px solid #cccccc";
		    document.getElementById('searchGoButton').src="/law-products/_ui/swc/images/search-grey_ICN.png";
		    
		    $("#searchfield").css("border-top", "1px solid #cccccc");
		    $("#searchfield").css("border-bottom", "1px solid #cccccc");
		    $("#searchfield").css("border-left", "1px solid #cccccc");
		    $("#searchfield").css("border-right", "0");
		    $("#searchfield").css("width", "140px");
		    $("#searchfield").css("color","#999999");	
		});
	  
	});
		
</script>


