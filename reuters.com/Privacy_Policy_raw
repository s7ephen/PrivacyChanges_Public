<script>
  $(document).ready(function () {
    var stickyNavTop = $('#c-nav-wrap').offset().top;

    var stickyNav = function () {
      var scrollTop = $(window).scrollTop();

      if (scrollTop > stickyNavTop) {
        $('#c-nav-wrap').addClass('sticky');
      } else {
        $('#c-nav-wrap').removeClass('sticky');
      }
    };

    $(window).scroll(function () {
      stickyNav();
    });
  });
</script><link href="http://fonts.googleapis.com/css?family=Roboto:400,300,700" rel="stylesheet" type="text/css" />
