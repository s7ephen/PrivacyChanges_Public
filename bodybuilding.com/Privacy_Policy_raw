    var BB;
    BB = window.BB || {};
    BB.Instances = BB.Instances || {};
    //Storing instance of tip in BB namespace for later use
    BB.Instances.tsTip = $.tipSwift.plugins.tip({
        showOn: [],
        hideOn: [],
        extraClass: ['closebtntop'],
        title: function(){
            return "<p class=\"messages\">A minimum of 5 characters must be entered in order to post.<p><a  class=\"close\"></a>";
        },
        html:'html',
        gravity:'s'
    });

    //Initialize tooltips
    $('.bodyspace-comment-submit-button a.post').tipSwift({
        plugins: [BB.Instances.tsTip]
    });

    $(document).ready(function() {
        $('textarea.autosize').textareaAutoheight();
        $('#message-box-comment-confirm').hide();
        $('#message-box-comment-error').hide();

        var onEditCallback = function(remaining){
            //return false;
        }

        var onLimitCallback = function(){
            return false;
        }

        $('textarea').limitMaxlength({
            onEdit: onEditCallback,
            onLimit: onLimitCallback
        });

        $('.counter-comments').each(function(index, obj){
            if (index == 1 || index ==3 )
            {
                $(this).removeClass('counter-comments');
                $(this).addClass('total');
            }
        });

        setTimeout(function() {
            $.clearwatermarks();
            $.addwatermarks();
        }, 300);

    });



}());

});
