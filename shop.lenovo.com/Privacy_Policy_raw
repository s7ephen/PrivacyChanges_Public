				var cartCount = $.cookie("_CartCount");
				if(typeof cartCount == 'string'){
					$(".bar_2-link-cart").find('.number-flag').empty().html(cartCount).show();
					var counter = $(".cartlink");
					$.each(counter,function(){
						$(this).children().append("<span class='counter'></span>");
						$(this).children().find('.counter').empty().html('&nbsp;('+cartCount+')').show();
					});
				}
