				function showInPopup(wURL, wFeatures) {
					if (!wFeatures) {
						wFeatures = 'height=420, width=800, resizeable=yes, modal=yes, alwaysRaised=yes';
					}
					var wAnswer, wParams = '';
					wAnswer=window.open(wURL, wParams , wFeatures);
