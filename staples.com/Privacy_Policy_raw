	<script type="text/javascript">
		function openLinkInWindow(url,event,source){
			if(source=="DD"){
				try{
					var anlText=$("#headerExt .dailyDealsSection").find("div").data("analytics");
					s_flyoutTrack(anlText, false);
				}catch(err){}
			}
			location.href=url;
		}
	</script>

	

