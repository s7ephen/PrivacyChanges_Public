<script type="text/javascript">

$('#topic_select').bind('change', function() {
	
	$('.form_display_sections').hide();
	
	if ($('select[id^="' + $(this).val() + '"]').length) {
		$('select[id^="' + $(this).val() + '"]').show().get(0).selectedIndex = 0;
	} else {
		$('select.form_display_sections:last').show();
	}
	
	$('div[id^="' + $(this).val() + '"].form_display_sections:first').show();
	
});

$('select.form_display_sections').bind('change', function() {
	$('div.form_display_sections').hide();
	$('div[id^="' + $(this).attr('id') + '.' + $(this).val() + '"]').show();
});

$('form.cs_module select').bind('change', function() {
	
	if ($('#topic_select').val() == 'your_order' && $('#your_order').val() == 'return_order_for_credit_or_exchange_items') {
		$('input[name="order_number"]').next().show();
	} else {
		$('input[name="order_number"]').next().hide();
	}
	
	if ($('#topic_select').val() == 'staples_rewards') {
		$('input[name="rewards_number"]').parent().show();
	} else {
		$('input[name="rewards_number"]').parent().hide();
	}
	
	if ($('#topic_select').val() == 'product_inquiries') {
		$('input[name="zip_code"]').parent().show();
	} else {
		$('input[name="zip_code"]').parent().hide();
	}
	
	if ($('#topic_select').val() == 'store_questions' || $('#topic_select').val() == 'store_complaints') {
		$('form.cs_module').removeClass('cs_closed');
	} else {
		$('form.cs_module').addClass('cs_closed');
	}
	
	if ($('#topic_select').val() == 'store_compliments') {
		document.location.href = 'http://staples.force.com/CustomerFeedback/';
	}
	
	$('form.cs_module input[type="text"]:not(:visible)').attr('disabled', 'disabled');
	$('form.cs_module input[disabled]:visible').removeAttr('disabled');
	
	if ($('#topic_select').val() == 'your_order') {
		
		if ($('#your_order').val() == 'return_order_for_credit_or_exchange_items') {
			$('input[name="emailTo"]').val('return@orders.staples.com');
			
		} else if ($('#your_order').val() == 'missing_items') {
			$('input[name="emailTo"]').val('missingitem@orders.staples.com');
			
		} else if ($('#your_order').val() == 'order_is_late') {
			$('input[name="emailTo"]').val('lateorder@orders.staples.com');
			
		}
		
	} else if ($('#topic_select').val() == 'payment_credit') {
		
		if ($('#payment_credit').val() == 'refund_status') {
			$('input[name="emailTo"]').val('refund@orders.staples.com');
			
		} else if ($('#payment_credit').val() == 'order_history_packing_slip') {
			$('input[name="emailTo"]').val('orderhistory@orders.staples.com');
			
		} else if ($('#payment_credit').val() == 'price_match') {
			$('input[name="emailTo"]').val('pricematch@orders.staples.com');
			
		} else if ($('#payment_credit').val() == 'coupon_questions') {
			$('input[name="emailTo"]').val('coupon@orders.staples.com');
			
		} else if ($('#payment_credit').val() == 'rebate_question') {
			$('input[name="emailTo"]').val('rebates@orders.staples.com');
			
		} else if ($('#payment_credit').val() == 'tax_question') {
			$('input[name="emailTo"]').val('tax@orders.staples.com');
			
		} else if ($('#payment_credit').val() == 'staples_charge_account_status') {
			$('input[name="emailTo"]').val('staplescharge@orders.staples.com');
		
		}
		
	} else if ($('#topic_select').val() == 'product_inquiries') {
		$('input[name="emailTo"]').val('products@orders.staples.com');
		
	} else if ($('#topic_select').val() == 'business_services') {
		$('input[name="emailTo"]').val('experts@orders.staples.com');
		
	} else if ($('#topic_select').val() == 'website_assistance') {
		$('input[name="emailTo"]').val('website@orders.staples.com');
		
	} else if ($('#topic_select').val() == 'store_questions' || $('#topic_select').val() == 'store_complaints') {
		$('input[name="emailTo"]').val('retail@orders.staples.com');
		
	} else if ($('#topic_select').val() == 'catalog_questions') {
		$('input[name="emailTo"]').val('support@orders.staples.com');
		
	} else if ($('#topic_select').val() == 'staples_rewards') {
		$('input[name="emailTo"]').val('dividends@staples.com');
		
	} else if ($('#topic_select').val() == 'something_else') {
		$('input[name="emailTo"]').val('other@orders.staples.com');
		
	}
	
});

var label, value = '';

$('form.cs_module').bind('submit', function() {
	
	$('input[name="emailFrom"]').val($('input[name="email"]').val());
	$('input[name="emailSubject"]').val($('form.cs_module select[name="topic_select"] option:selected').text());
	
	if ($('form.cs_module select[name="category_select"]:visible option:selected').text()) {
		$('input[name="emailSubject"]').val($('input[name="emailSubject"]').val() + ' - ' + $('form.cs_module  select[name="category_select"]:visible option:selected').text());
	}
	
	$('input[name="emailBody"]').val('');
	
	$('form.cs_module select:visible, form.cs_module input:visible').each(function() {
		
		label = $('label[for="' + $(this).attr('name') + '"]').text();
		
		if ($(this).get(0).nodeName == 'SELECT') { value = $(this).find('option:selected').text(); }
		else { value = $(this).val(); }
		
		$('input[name="emailBody"]').val($('input[name="emailBody"]').val() + label + ' ' + value + "\n");
		
	});
	
	$('input[name="emailBody"]').val($('input[name="emailBody"]').val() + "\n" + $('textarea[name="body"]').val() + "\n");
	
	$(this).find('fieldset select, fieldset input').removeAttr('name');
	
	var required_flag = false;
	
	$('span.cs_required:visible').each(function() {
		
		if (!$(this).prev().val()) { required_flag = true; }
		
	});
	
	if (required_flag) { alert('Some required fields are empty.'); return false; }
	
	return true;
	
});

$(document).ready(function() {
	$('input[name="emailTo"]').val('return@orders.staples.com');
	$('form.cs_module select').each(function() { this.selectedIndex = 0; });
	$('select.form_display_sections:first, div.form_display_sections:first').show();
	$('input[name="rewards_number"], input[name="zip_code"]').parent().hide();
	$('form.cs_module input[type="text"]:not(:visible)').attr('disabled', 'disabled');
});

</script>


