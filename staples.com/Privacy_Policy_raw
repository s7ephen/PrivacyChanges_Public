	<div class="footerFloaters">
		<div id="footerEmailSignup">
			<span>Deals! Get them now</span>
						<script type="text/javascript">
							/*
							Prevent context menu for all:
								oncontextmenu="return false;"
							Prevent ctrl-v:
								IE &amp; Webkit (Safari, Konqueror, etc.):
									onpaste="return false;"
								For Gecko (Firefox, Mozilla, Camino, etc.):
									onkeypress="return preventPaste(event);"
							*/
							function preventPaste(evt) {
								var isCtrl;
								var keyCode;
								var keyLetter;

								isCtrl = (evt.ctrlKey) ? true : false;
								keyCode = evt.charCode;
								keyLetter = String.fromCharCode(keyCode).toLowerCase();

								if(isCtrl && keyLetter == "v") { evt.preventDefault(); }
							}
						</script>
		</div>
		<div id="innerSignUp">
			<form name="EmailSubscribeFormFooter" method="post" action="http://www.staples.com/office/supplies/StaplesEmailSubscriptionChange?">
				<label for="email">Email Address *</label>
					<input id="email" class="footerInput" name="email1" maxlength="256" type="text">
				<label for="reenteremail">Confirm Email Address *</label>
					<input id="reenteremail" class="footerInput" name="newReEnterEmailAddr" maxlength="256" onpaste="return false;" oncontextmenu="return false;" onkeypress="return preventPaste(event);" type="text">
				<a href="javascript:STAPLES.Main.formSub('EmailSubscribeFormFooter');" class="nonPurchaseBtn headerBtn" data-analytics="Footer:Deals! Get them now">SUBMIT</a>
