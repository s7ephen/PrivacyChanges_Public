<div class="article-header"><div class="title">

<div class="container-fluid">
            <div class="row-fluid">
                <div class="span12">
                    <h1><span>Web & Mobile Privacy Statement</span></h1>
                </div>
            </div><!--/.row-fluid-->
           </div><!--/.container-fluid--></div>
</div>
<div class="template article-template">
  <div class="container-fluid">    
    <div class="row-fluid breadcrumb">
		<div class="span10">
        	
	         	
	         
        	

<ul><li class="noLink">Aetna</li>       
        <li>
            
	            	<a href="/legal-notices.html">Legal Notices</a>
	            	<span class="bread-arrow"></span>
	            
        </li>       
        <li>
            
	            	<a href="/legal-notices/privacy.html">Privacy</a>
	            	<span class="bread-arrow"></span>
	            
        </li>       
        <li>
            
            	<span>Web & Mobile Privacy Statement</span>
            
        </li></ul>
        </div>
        
	        <div class="span2">
				
		         	
		        
	         	






    
		<ul class="printShare">
			<li class="noLink printPage">
				<a href="">Print</a>
			</li>
			<li>
				<a id="util-bookmarks" href="http://www.addthis.com/bookmark.php" style="text-decoration:none;" onmouseover="return addthis_open(this, '', '[URL]', '[TITLE]');" onmouseout="addthis_close();" onclick="return addthis_sendto();">Share</a>
