    cmg.query(document).ajaxError(function(evnt, jqxhr) {
        var error;
        try { error = JSON.parse(jqxhr.responseText); }
        catch(e) { error = jqxhr.responseText; }
        console && console.error && console.error('ajax error:', error);
    });
    
    cmg.error_dialog = function(msg) {
        cmg.query('<div/>').addClass('cmErrorDialog')
            .text(msg = msg || 'Oops, something went wrong. Please reload the page and try again.')
            .appendTo('body');
    };
    cmg.get_cookie = function get_cookie(name) {
        var cookie_array = document.cookie.split(';');
        var cookie_regex = new RegExp('^ *' + name + '="?([^"]+)"?');
        for (var x in cookie_array) {
            if(!cookie_array.hasOwnProperty(x)) continue;
            var match = cookie_array[x].match(cookie_regex);
            if(match){
                return match[1];
            }
        }
    };
    cmg.parse_qs = function parse_qs(str) {
        if (!str) { return {}; }
        if (str[0] === '?') { str = str.slice(1); }
        var ret = {};
        var query_args = str.split('&');
        for (var key in query_args) {
            if (!query_args.hasOwnProperty(key)) { continue; }
            var tuple = query_args[key].split('=');
            ret[tuple[0]] = tuple[1];
        }
        return ret;
    };
    
    cmg.query.ajax_includes = function(fun) { cmg.query(document).bind('ajax_includes', fun); };
    
    var metrics_signin = 0;

    
    cmg.query.ajaxSetup({ cache: true });
</script><script>

cmg.get_profile_avatar = function(user_name) {
    return new Promise(function(resolve, reject) {
        // Attempt to return a cached URL
        var url = cmg.localCache.load('avatar');
        if (typeof url === 'string') {
            resolve(url);
            return;
        }

        // Query the server for the user's current avatar URL
        var uuid = janrain.capture.ui.getReturnExperienceData('uuid') || '';
        cmg.query.ajax({
            url: 'http://www.springfieldnewssun.com/auth/ajax/profile-avatar',
            dataType: location.host === 'www.springfieldnewssun.com' ? 'json' : 'jsonp',
            data: { u: uuid, nmredir: 'true' },
            success: function(data) {
                var url = data && data.avatar_url || '';
                cmg.localCache.store('avatar', url, { expires: 300 });
                resolve(url);
            },
            error: function() {
                console.error(new Error('An error occurred while attempting to retrieve profile avatar'));
                resolve('');
            }
        });
    }).then(function(url) {
        if (!url) {
            url = 'http://www.springfieldnewssun.com/profile/avatar_jp2/' + encodeURIComponent(user_name) + '/';
        }
        return url;
    });
};

cmg.update_auth_message = function(str) {
    function enableHover() {
        var $target = cmg.query('.cm-user-hover-target'),
            menu_timer;

        if ($target.hasClass('cm-janrain-active')) {
            return;
        }

        $target.addClass('cm-janrain-active');

        $target.hover(function() {
            var $this = this;
            menu_timer = setTimeout(function() {
                if (cmg.query($this).is(":hover")) {
                    cmg.query('#cm-janrain-overlay-container').show();
                }
            }, 500);
        }, function() {
            var $this = this;
            menu_timer = setTimeout(function() {
                if (!cmg.query($this).is(":hover")) {
                    cmg.query('#cm-janrain-overlay-container').hide();
                }
            }, 500);
        });
    }

    var cookie_name = cmg.query.cookie('ur_name'),
        views_total_count = cmg.query.cookie('ur_views_total_count', {path:'/'}),
        $icon = cmg.query('#cm-user-icon');

    if (typeof cookie_name === 'string' && typeof views_total_count == 'string'
            && (typeof janrain !== 'undefined') && janrain.capture && janrain.capture.ui
            && janrain.capture.ui.hasActiveSession()) {
        cmg.query('#cmUserNameUpdate').text(cookie_name);
        cmg.get_profile_avatar(cookie_name).then(function(url) {
            $icon.css({'background-image': 'url(' + url + ')', 'display': 'block'})
                .removeClass('cmOpenJanrainModal cm-icon cm-icon-rounded cm-icon-user')
                .addClass('cm-user-avatar');
        });
        enableHover();
    } else {
        $icon.css({'background-image': 'none', 'display': 'block'})
            .attr('href', '#')
            .removeClass('cm-user-avatar')
            .addClass('cmOpenJanrainModal cm-icon cm-icon-user');
    }
};

