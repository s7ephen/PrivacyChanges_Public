        <script>
          function loadCSS(url) {
            var link = document.createElement("link");
            link.type = "text/css";
            link.rel = "stylesheet";
            link.href = url;
            document.getElementsByTagName("head")[0].appendChild(link);
          }
          setTimeout(function(){
            loadCSS("http://nodeassets.today.com/styles/e9751715.secondary.css");
          },0);
        </script>
    

