var iOS = /(iPad|iPhone|iPod)/g.test( navigator.userAgent );
var af_lazyloading_timeout = null;
var af_lazyloading_dist = iOS ? 1000 : 200;
var af_lazyloading_imgs = document.getElementsByTagName("img");

var af_html_lazyloading_dist = iOS ? 1200 : 500;
var af_html_lazyloading_items = new Array();
var af_html_lazyloading_callback = new Array();

function af_lazyloading() {
    if (af_lazyloading_timeout === null) {
        af_lazyloading_timeout = setTimeout(af_lazyloading_real, 50);
    }
}
function af_lazyloading_real() {
	var ws = af_windowSize();
	var imgs = document.querySelectorAll("img");
	var len = imgs.length;
	var img, dataOrigin, visibility;
	for (var i = 0; i < len; i++) {
		img = imgs[i];
		dataOrigin = img.getAttribute("data-origin");
		// Do not lazy-load hidden images
		display = (img.currentStyle ? img.currentStyle.display : getComputedStyle(img, null).display);
		visibility = (img.currentStyle ? img.currentStyle.visibility : getComputedStyle(img, null).visibility);
		if (dataOrigin 
				&& visibility !== "hidden" && display !== "none" 
				&& ws.b + af_lazyloading_dist >= af_findPos(img).t) {
			img.setAttribute("data-origin", "");
			af_addEvent(img, "load", (function() {
					this.classList.add("af-lazyloaded");
					setTimeout((function() { this.classList.remove("af-lazyload"); }).bind(this), 500);
				}).bind(img), false);
			img.src = dataOrigin;
		}
	}
	var len = af_html_lazyloading_items.length;
	for (var i = 0; i < len; i++) {
		var item = af_html_lazyloading_items[i];
		var callback = af_html_lazyloading_callback[i];
		if (callback !== null) {
			if (ws.b + af_html_lazyloading_dist >= af_findPos(item).t) {
				callback(item);
				af_html_lazyloading_callback[i] = null;
			}
		}
	}
	af_lazyloading_timeout = null;
}
if (typeof(af_addEvent) != "undefined") {
                af_addEvent(window, "scroll", af_lazyloading, false);
                af_addEvent(window, "load", af_lazyloading, false);
}
</script>
