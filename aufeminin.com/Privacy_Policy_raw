<script type="text/javascript">
	if (typeof(af_addEvent) != "undefined") {
		af_addEvent(window, "scroll", af_lazyloading, false);
		af_addEvent(window, "load", af_lazyloading, false);
	}
	
	function af_newsletterSubmit() {
		var af_em=document.getElementById('newsletterEmail').value;
			
		if (!af_em.match(/^[a-z0-9-_\.]{2,}@([a-z0-9-]{2,}\.)+[a-z]{2,3}$/i)) {
			alert('Please correct your email in order to receive the aufeminin newsletter!');
			document.getElementById('newsletterEmail').focus();
			return false;
		} else {
			af_requestGet('/world/communaute/users/newsletter_inscription.asp','ajax=1&EmailActuel=' + af_em + '&NLID='+document.getElementById('newsletterID').value, function(data){
				if (data.error == ''){
					document.getElementById('newsletterContainer').innerHTML = '<p id=\"text-full-line\">'+data.message + '<br/><strong>'+data.messageDesc+'</strong></p>';
				} else {
					console.log(data);
				}
			}, true);
			return false;
		}
	}
</script>

