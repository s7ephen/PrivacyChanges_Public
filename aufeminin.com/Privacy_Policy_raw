function setAfXperso(cName,name,value,expires,path,domain,secure){
	var c='', i=0, nv=name+'='+escape(value), ckt=document.cookie.split(';');
	for (i=0;i<ckt.length;i++) { var ck=ckt[i]; while (ck.charAt(0)==' ') ck=ck.substring(1,ck.length); if (ck.substr(0,cName.length+1)==cName+'=') { c=ck.substring(cName.length+1,ck.length); break; } }
	if (c.length==0) c=nv; else { var cvt=c.split('&'),vi=-1; for (i=0;i<cvt.length;i++) if (cvt[i].substr(0,name.length+1)==name+'=') { vi=i; break; } if (vi<0) c=c+'&'+nv; else { cvt[vi]=nv; c=cvt.join('&'); }}
