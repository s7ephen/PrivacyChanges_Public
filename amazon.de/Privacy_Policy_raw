<script type="text/javascript">

P.when('jQuery', 'ready').execute(function(jQuery) {
    jQuery('a.expander').bind(
        'click',
        function(event) {
            event.preventDefault();
            jQuery(this).parent().next().children().toggleClass('cs-none');
            jQuery(this).toggleClass('close');
        }
    );
});

P.when('jQuery', 'a-popover', 'ready').execute(function(jQuery, popover) {
    jQuery('.cs-help-pop-link').each(function() {
        var localContent = jQuery(this).attr('name') + '-content';
        if (localContent) {
            jQuery(this).addClass('cs-link-hover');
            popover.create(jQuery(this), {
                closeButton: false,
                position: 'triggerVerticalAlignLeft',
                content: jQuery('#' + localContent).html()
            });
        }
    });
});

</script>



