                addonCall('openLoginPage', { url:linkedinuser.url}, function (iresponse) { });

            } else if( thearray[id].click == undefined ){ 

                defaultIssueClick(id, force); //default fbissue button click behavior

            } else {

                thearray[id].click(force);
            }                
        
        });

