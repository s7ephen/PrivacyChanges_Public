                //console.log('HEARD in PRIVACYFIX PAGE: ' + response.eventname);
                //console.log(response);
                if (callbacks[response.eventid] != undefined) {
                    var fn = callbacks[response.eventid];
                    fn(response);
                } else {
                    //console.log('No callback function.');
                }
            });



