    function flagForUpdate(groupname){

        for (var k in thearray) {
            if ( thearray[k].group == groupname ) {
                thearray[k].refresh = true;
            }
        }

    }

    function disableGroup(groupname){

        for (var k in thearray) {
            if ( thearray[k].group == groupname ) {
                thearray[k].enabled = false;
            }
        }
    }

