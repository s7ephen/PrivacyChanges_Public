<script>

function loadNav()
{
if (!window.ucs) {
    window.ucs = {}; 
}
window.ucs.api = window.ucs.api || {}; 

YUI.add('ucs-inject-mobile', function (Y) {

    "use strict";
    
    Y.namespace('ucs');
 
   
    var ucm                     = Y.ucs.ConfigMobile,
        utils                   = Y.ucs.UtilsMobile,
        YArray                  = Y.Array,
        YObject                 = Y.Object,
        YLang                   = Y.Lang;

    Y.ucs.InjectMobile = function (Y) {
        this._initCustomItems();
        
    };
    Y.ucs.InjectMobile.prototype = {
        _initCustomItems: function() {
            Y.Global.once(ucm.NAV_TOGGLE_EVT, function(e) {
                var et = new Y.EventTarget();                                         
				
  

     			
    
   
  
  
  



               	var data = {'data':[
			
				
           

               
                  	
                        	
 
				
							
				
 
   	
   	
  

 
    
   
  
  
  



  

               
					  {
                        'title': 'Terms',
                        'link': {
                            'url': '/ie/en/yahoo/terms/utos/index.htm'
                        		}
                        		
                        		
                      	
                        	
 
				
							
				
 
   	
   	
   	 
                      },
	  

 
    
   
  
  
  



  

               
					  {
                        'title': 'Privacy Centre',
                        'link': {
                            'url': '/ie/en/yahoo/privacy/index.htm'
                        		}
                        		
                        		
                      	
                        	
 
				
							
				
 
   	
   	
   		  

 
    
   
  
  
  



  

               
                  	
                        	
				          
                        			,'navigation':[
				                              
                                			 {
												'title': 'Topics',
												'link': {
													'url': '/ie/en/yahoo/privacy/topics/index.htm'
														}
											 }
				 
				
							
				
		                                                                                                     
                             			  ,
				 
		    
				  
			 
   	
   	
  

 
    
   
  
  
  



  

               
                  	
                        	
				                              
                                			 {
												'title': 'Products',
												'link': {
													'url': '/ie/en/yahoo/privacy/products/index.htm'
														}
											 }
				 
				
							
				
		         
		    
				   
										]},
				  
			 
   	
   	
  

 
    
   
  
  
  



  

               
					  {
                        'title': 'Copyright/IP',
                        'link': {
                            'url': '/ie/en/yahoo/ip/index.htm'
                        		}
                        		
                        		
                      	
                        	
 
				
							
				
 
   	
   	
   		  

					  }					
		                           ]
                             };	  
	 
                et.publish(ucm.ADD_NAV_ITEMS, {broadcast:2,emitFacade:true});
                et.fire(ucm.ADD_NAV_ITEMS, data);
            });
        }
    }
}, '3.0', {
    requires: ['node', 'ucs-navigation-mobile', 'event-custom']
});

(function () {

    "use strict";
    
    YUI().use('node','event-custom', 'ucs-inject-mobile', function (Y) {
    
        var container = Y.one('#' + Y.ucs.ConfigMobile.NAV_CONTAINER);

        if (container && !window.ucs.inject) {
    
            window.ucs.inject = new Y.ucs.InjectMobile(container);
            //Y.Global.fire(Y.ucs.ConfigMobile.API_NAV_READY, window.ucs.api);
        }   
    
    }); 
}());
}
