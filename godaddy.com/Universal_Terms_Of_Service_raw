<script type="text/javascript">
function atl_GetQuickHelpUrl() { return "https://www.godaddy.com/external/quickhelp/quickhelpitem.aspx"; }
function getHelpTipText(qhid, elem) {
var u = atl_GetQuickHelpUrl();
u += (u.indexOf('?') >= 0) ? "&" : "?";
u += "TargetDivID=qh&textonly=1&name=" + qhid;
$.ajax({ type: "GET", url: u, dataType: "json", async: false }).done(function (data) { if (data != null) { elem.attr("data-qhtext", "<p>" + data.Html + "</p>"); }; });
}
$(".help-tip, .help-text").live("mouseover", function () {
if (!$(this).data("set")) {
if ($.isFunction($.fn.qtip)) {
var qhtext = "";
if ($(this).attr("data-qhid")) {
getHelpTipText($(this).attr("data-qhid"), $(this));
}
if ($(this).attr("data-qhtext")) {
qhtext = $(this).attr("data-qhtext");
}
var qhtitle = $(this).attr("data-qhtitle") ? (($(this).attr("data-qhtitle").length == 0) ? "-" : $(this).attr("data-qhtitle")) : "-";
$(this).qtip({
position: { my: "left top", at: "right center", viewport: $(window) },
content: { text: qhtext, title: { text: qhtitle, button: true} },
style: { width: "300" },
show: { event: "click" },
hide: { event: false },
events:
{
render: function (event, api) {
var tooltip = api.elements.tooltip;
if (qhtitle == "-") {
tooltip.find("div.ui-tooltip-title").remove();
tooltip.find("div.ui-tooltip-titlebar").css({ "padding-bottom": "0" });
tooltip.find("div.ui-tooltip-content").css({ "padding-top": "0", "top": "-12px" });
}
}
}
}).data("set", true);
}
}
});
</script>
