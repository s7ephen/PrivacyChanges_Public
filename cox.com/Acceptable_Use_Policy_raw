		    <!-- UDO Data Store -->
		    <script type="text/javascript">
if ( !CQ_Analytics.UDO ) {
	CQ_Analytics.UDO = new CQ_Analytics.SessionStore();
	CQ_Analytics.UDO.initialized = false;

	CQ_Analytics.UDO.STOREKEY = "UDO";
	CQ_Analytics.UDO.STORENAME = "udo";
	CQ_Analytics.UDO.data={
			"businessUnit":"",
			"campaign":"",
			"campaignCodeInternal":"",
			"categorySubscribed":"",
			"categoryViewed":"",
			"serviceableCodes":"",
			"channel":"",
			"cidm":"",
			"dateStamp":"",
			"language":"",
			"localeName":"",
			"lpHomeSecuritySubscribed":"",
			"lpInternetSubscribed":"",
			"lpPhoneSubscribed":"",
			"lpTVSubscribed":"",
			"pageName":"",
			"pageType":"",
			"productID":"",
			"productMRC":"",
			"productMRCSavings":"",
			"productOfferID":"",
			"productOfferMonths":"",
			"productOfferMRC":"",
			"productOfferName":"",
			"productOfferSavings":"",
			"productOneTimeChargeSavings":"",
			"productSubscribed":"",
			"productViewed":"",
			"promoCode":"",
			"purchaseStep":"",
			"responsiveDisplayType":"",
			"searchResults":"",
			"searchTerm":"",
			"siteID":"",
			"subSection":"",
			"tabName":"",
			"toolName":"",
			"visitorAuthenticatedUserRole":"",
			"visitorLoginStatus":"",
			"visitorServiceability":"",
			"visitorType":"",
			"websiteEngagementScore":"",
			"zip":""
	};


	//register the session store
	if (CQ_Analytics.ClientContextMgr){
		CQ_Analytics.ClientContextMgr.register(CQ_Analytics.UDO)
	}

	//Need to update udoUpdate to NOT copy UDO elements if they are empty
	CQ_Analytics.UDO.udoUpdate = function() {
		for (var key in this.data) {
			if (this.data.hasOwnProperty(key)) {
				if(typeof key != 'undefined' && this.data[key]!=null && this.data[key]!=""){
					window["utag_data"][key] = this.data[key];
				}
			}
		}
	};

	CQ_Analytics.UDO.init = function() {
		this.addListener("update", this.udoUpdate);

		if (typeof window.utag_data == 'undefined') {
			window["utag_data"] = {};
		}

	};

	if(CQ_Analytics.ClientContextUtils) {
		CQ_Analytics.ClientContextUtils.onStoreRegistered("udo", CQ_Analytics.UDO.init);
		CQ_Analytics.UDO.initialized = true;
	}

	$.urlParam = function(name){
	    var results = new RegExp('[\\?&]' + name + '=([^&#]*)').exec(window.location.href);

	  if(results== null){
	    	return "";
	    }
	    else{
	    	return results[1];
	    }
	}
	var segments = window.location.pathname.split('/');

	pageName = window.location.pathname.split('/')[segments.length-1];
	pageName = segments[2] + ":";


	if(segments[1] === "residential"){
		channelName = "cox:res";
		businessUnit = "res";
		}
		else if(segments[1] === "business") {
		channelName = "cox:bus";
		businessUnit = "bus";
		}
		else if(segments[1] === "aboutus"){
			channelName = "cox:aboutus";
			businessUnit = "aboutus";
		}
		else{
			channelName = "cox:dev";
			businessUnit = "dev";
		}

	pageName = channelName;
	subSection = channelName;

	for(var i=2;i<segments.length;i++){
	  pageName = pageName + ":" + segments[i];
	}
	for(var i=2;i<segments.length-1;i++){
	  subSection = subSection + ":" + segments[i];
	}
	if(segments.length>3){
		channelName = channelName + ":" + segments[2];
	}
	pageName = pageName.substring(0,pageName.length-5);

		console.info("Setting UDO Parameters");

		var mq = window.matchMedia('all and (max-width: 766px)');
		if (mq.matches)
		{
			CQ_Analytics.UDO.setProperty("responsiveDisplayType", 'mobile');
		}
		else{
			CQ_Analytics.UDO.setProperty("responsiveDisplayType", 'desktop');
		}


		CQ_Analytics.UDO.setProperty("pageName", pageName);
		CQ_Analytics.UDO.setProperty("subSection", subSection);
		CQ_Analytics.UDO.setProperty("businessUnit", businessUnit);
		CQ_Analytics.UDO.setProperty("channel", channelName);

		CQ_Analytics.UDO.setProperty("campaign",$.urlParam('sc_id'));
		CQ_Analytics.UDO.setProperty("campaignCodeInternal",$.urlParam('campcode'));

		CQ_Analytics.UDO.setProperty("campaignSession", $.urlParam('sc_id'));
		CQ_Analytics.UDO.setProperty("campaignCodeInternalSession", $.urlParam('campcode'));
		CQ_Analytics.UDO.setProperty("promoCode", $.urlParam('PC'));
		CQ_Analytics.UDO.setProperty("purchaseStep", "mktg");
		CQ_Analytics.UDO.setProperty("pageType", "mktg");
		CQ_Analytics.UDO.setProperty("language", "en");
		if (jQuery("meta[name='categoryViewed']").length > 0) { CQ_Analytics.UDO.setProperty("categoryViewed",jQuery("meta[name='categoryViewed']").attr("content").split(",")) };

}

</script>
			


 <script type="text/javascript">
        (function() {
          var profileStore = CQ_Analytics.ClientContextMgr.getRegisteredStore("udo");
          if(profileStore){
            profileStore.setProperty("zip", "10020");
            profileStore.setProperty("franchiseId", "");
			profileStore.setProperty("customerType", "unknown\x20\u2013\x2010020");
			profileStore.setProperty("address", "");
			profileStore.setProperty("siteID", "");
            profileStore.setProperty("cidm", "");
			profileStore.setProperty("visitorAuthenticatedUserRole", "");
			profileStore.setProperty("visitorLoginStatus", "anonymous");
			profileStore.setProperty("visitorServiceability", "nonserviceable");
			profileStore.setProperty("visitorType", "noncustomer");
			profileStore.setProperty("localeName", "");
			profileStore.setProperty("lpHomeSecuritySubscribed", "");
			profileStore.setProperty("lpInternetSubscribed", "");
			profileStore.setProperty("lpPhoneSubscribed", "");
			profileStore.setProperty("lpTVSubscribed", "");
			profileStore.setProperty("productID", parseJSON(''));
			profileStore.setProperty("productMRC", parseJSON(''));
			profileStore.setProperty("productMRCSavings", parseJSON(''));
			profileStore.setProperty("productOfferMonths", parseJSON(""));
			profileStore.setProperty("productOfferMRC", parseJSON(""));
			profileStore.setProperty("productOfferID", parseJSON(""));
			profileStore.setProperty("productOfferMonths", parseJSON(""));
			profileStore.setProperty("productOfferMRC", parseJSON(""));
			profileStore.setProperty("productOfferName", parseJSON(""));
			profileStore.setProperty("productOfferSavings", parseJSON(""));
			profileStore.setProperty("productOneTimeChargeSavings", parseJSON(""));
			profileStore.setProperty("productOneTimeCharge", parseJSON(""));
			profileStore.setProperty("productSubscribed",  parseJSON(""));
			profileStore.setProperty("productViewed", parseJSON(""));
			profileStore.setProperty("categorySubscribed",  parseJSON(""));
			profileStore.setProperty("serviceableCodes", "");
			profileStore.setProperty("dateStamp", "1441999756312");
          }
        }
        )();
        function parseJSON(str){
        	if(str.length >0){
	    		var returnStr = $.parseJSON(str);
	    		return returnStr;
    		}
    		return str;
        }
</script>

