  function eventerBind(selector) {
    $('body').on('click', selector, function(e) {
      var $data = $(this).data();
      eventer(
        EVENTER_RECOMMENDATION_CLICK,
        $data['p13nRecommendationId'],
        $data['labelId'],
        $data['position']
        );
    });
  };

