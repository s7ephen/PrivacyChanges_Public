    <script type="text/javascript" charset="utf-8">
$(function() {
  window.CommissionJunction = {
    hide_coupons: function(){
      if (this.cj_ref_code_matches()) {
        $('[href$="/coupons"]').parent().hide();
      }
    },
    cj_ref_code_matches: function() {
      var cookie, ref_code;
      cookie = document.cookie.match(/ref_code=([^;]*)/);
      ref_code = cookie ? cookie[1] : "";
      return ref_code.match(/^XUA0000COMJ000000000000ENUS0000000000000021052012/);
    }
  }
  CommissionJunction.hide_coupons();
});
</script>

    

