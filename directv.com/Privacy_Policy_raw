function delimiter(input,delimiter) {
var delimitLowerCase = delimiter.toLowerCase();
for(var i=0;i<input.length;i++){
if (input.indexOf(delimiter)==0) {
input=input.substring(delimiter.length);
i+=delimiter.length-1;
} else if(input.indexOf(delimitLowerCase)==0) {
input=input.substring(delimitLowerCase.length);
i+=delimitLowerCase.length-1;
} else {
break;
}
}
return input;
}
</script>
