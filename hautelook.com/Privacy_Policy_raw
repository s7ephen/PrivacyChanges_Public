    (function(){
        var email = '';
        var memberCookie = Cookies.get('hauteLookMember');

        if (memberCookie) {
            var parsedCookie = JSON.parse(memberCookie);
            email = parsedCookie.email;
        }

        window.ODY = window.ODY || [];
        window.SLMetaData = window.SLMetaData || {
            email: email
        };
    })();
</script>
                <!-- END Sociable Labs -->

        <script>
            requireModule('hldesktop/app');
        </script>

            <script>
        requireModule('hldesktop/app').send("enableMarketingIdentifierProcessing");
    </script>

            <script>
        requireModule('hldesktop/app').send("notifyAnalytics", "page-viewed");
    </script>


    
                
    </body>
</html>
