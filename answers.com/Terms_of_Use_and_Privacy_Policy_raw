<script type="text/javascript">

//roundBotCorners('toolsIcons');
hbx.hrf="http://wiki.answers.com/";
var http_referer;

if (document.referrer != "" && hbx.hrf == "") {
    http_referer = document.referrer;

    // get host name from URL
    matches = http_referer.match(/^(http:\/\/)?([^\/]+)/i);
    host = matches[2];

    if(host.match(/(google|yahoo|aol|msn|ask|dogpile|altavista|netscape|earthlink|myway|icerocket|answers)/i)) {
        hbx.hrf="http://" + host + "/";
        http_referer = "http://" + host + "/";
    }
    else {
    	hbx.hrf="http://other/";
    	http_referer = "http://other/";
    }
}


if (jQuery('#head_search').length > 0 && typeof(checkForCAPS) != 'undefined') {
	jQuery('#head_search').keypress(checkForCAPS.generic)
							.change(checkForCAPS.generic_check)
							.focus(checkForCAPS.focus);
}

var checkSubmit=checkSubmitUnified;
if(typeof jwgData.isLoggedIn=="undefined" || !jwgData.isLoggedIn){
jQuery("#header .signin_signup").show();
}
</script>
