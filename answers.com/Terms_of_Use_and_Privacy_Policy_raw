<script type="text/javascript">
var showFBLikeButton;
var fbRecommendations;
jQuery(window).load(function(){
	var addThisURL = '//s7.addthis.com/js/250/addthis_widget.js?domready=1';
	var quantserveURL = "//edge.quantserve.com/quant.js";
	loadAsyncSrc(addThisURL, loadAddThis);
	var site = "Answers";
	if (typeof ANSW!="undefined"){
		if (typeof ANSW.site!="undefined"){
			if(ANSW.site=="WA"){
				site = "Wiki"+site;
			}
		}
	}
	loadAsyncSrc(quantserveURL, loadQuantcast, site+"WikiAnswersOther");
	showFBLikeButton = true;
	fbRecommendations = true;
	loadFB();
});
</script>


