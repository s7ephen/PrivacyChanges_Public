    if(showRightbar){
		showAdmeldAd("rightBarTopDart","wikianswers","300x250","atf","",admeld_site);
		numAdsFromAdmeld++;
	}
    		    if(showRightbar){
    	showAdmeldAd("rightBarBotDart","wikianswers","300x250","btf","",admeld_site);
    	numAdsFromAdmeld++;
    }
        if(numAdsFromAdmeld>0){
   	   cv =  _hbEvent("cv");
	   cv.c23 = numAdsFromAdmeld+"|Legal Notices";
	   _hbDownload("dynamic");
    }

});
</script>
