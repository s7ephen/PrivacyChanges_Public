<script type="text/javascript">
var overlay = new EbaumOverlay(document.getElementById('highlightsList'),
			false);
EbaumGAHelper.addClickContainer(document.getElementById('highlightsList'));
jQuery(document).ready(function() {
    jQuery('#highlightsList').jcarousel({
    	scroll: 6,
    	wrap: 'circular',
    	initCallback:function() {
			jQuery('.highlightsContainer').css({'padding':'0', 'width': '970px'});
    	},
    	itemLastInCallback :function(carousel, item, index, action) {
        	if (index > 0) {
    			if (jQuery(item).next().find('img').attr('img-src')) {
    				var lis = jQuery(carousel.list).find('li img');
    				for (var i=index; i < index+6; i++) {
    					jQuery(lis).eq(i).attr('src', jQuery(lis).eq(i).attr('img-src')).removeAttr('img-src');
    				}
    			}
        	} else {
    			if (jQuery(item).prev().find('img').attr('img-src')) {
    				var lis = jQuery(carousel.list).find('li img');
    				var itemIndex = jQuery(item).index() - 5;
    				for (var i=itemIndex; i <= (jQuery(item).index()); i++) {
        				if (jQuery(lis).eq(i).attr('img-src')) {
    						jQuery(lis).eq(i).attr('src', jQuery(lis).eq(i).attr('img-src')).removeAttr('img-src');
        				}
    				}
    			}
        	}
    	}
        // Configuration goes here
    });
});
</script>

