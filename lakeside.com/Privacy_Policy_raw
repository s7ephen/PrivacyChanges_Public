			<script type="text/javascript">
				function fireTealiumPageView(data) {
					
					if(typeof utag === 'undefined') {
						console.warn('utag not defined; skipping event fire.');
					} else {
						if(typeof data != 'undefined') {
							console.log('firing tealium page view: ' + JSON.stringify(data));
							if (typeof(data) == 'string'){
								var x= JSON.parse(data);
								utag.view(x);
								}else{
									utag.view(data);
								}
						}
					}
				}

				function fireTealiumEvent(data) {
				//alert(JSON.stringify(data));
					if(typeof utag === 'undefined') {
						console.warn('utag not defined; skipping event fire.');
					} else {
						if(typeof data != 'undefined') {
							console.log('firing tealium event: ' + JSON.stringify(data));
							utag.link(data);
						}
					}
				}

				/*
				 * Fires an ajax call to tealium_ajax.jsp, passing in the json object, which is converted internally
				 * to k,v pairs.  Uses async=false to prevent in some cases that page from reloading before the call returns.
				 */
				function fireTealiumAjaxEvent(params) {
					try {
					if(typeof utag === 'undefined') {
						console.warn('utag not defined; skipping event fire.');
					} else {
						if(typeof params != 'undefined') {
							$.ajax({
								url: CONTEXT_ROOT + 'tagging/tealium_ajax.jsp',
								data: params,
								async: false,
								success: function(data) {
									//if(document.cookie.indexOf('tealium-debug') != -1) {
										console.log('url: ' + this.url);
										console.log('typeof data = ' + typeof(data));										
									//}
	
                                	if (typeof(data) == 'string'){
                                		var x= JSON.parse(data);
                                		utag.link(x);
                                	}else{
                                		utag.link(data);
                                	}
								}
							});
						}
					}
				}
				catch(err){
						console.log(err.message);
					}
			}

					$(window).load(function() {	
					if($('#tellAFriendConfirmationModal').length && $('#display').val() == 'Y') {
                    	fireTealiumEvent({"e_tell_friend" : "t"});
                    }

					
					if ($('body').hasClass('catconf')) {
						fireTealiumEvent({
							"e_catalog_request":"t",
							"e_lead_form_submit" : "t",
							"lead_type" : "request a free catalog confirm"
						});
					}
					
					
				/* event 81 - Email Unsubscribe event */
				if (window.location.href.indexOf("email_optout.jsp") != -1) {
			
					if($("#optOutEmail").length != 0){
			
					var emailvalue = $('#optOutEmail').val();
			
					if(emailvalue){
						fireTealiumEvent({
							"e_email_unsub" : "t"
						});
					 }
					}
				}					
					
					
				});
			</script>
		
	
