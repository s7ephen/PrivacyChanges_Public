<script type="text/javascript">

function setCookie(){
        // cookie expired in two weeks
                var today = new Date();
                today.setTime( today.getTime() );

                var expires = 14 * 1000 * 60 * 60 * 24;

                var exdate = new Date( today.getTime() + (expires) );
                var path="/";
        //debugger;
        if(navigator.appName.toLowerCase().indexOf('microsoft')!=-1){
                document.cookie="LTDSavedCartItems="+escape("null")+";expires="+exdate.toUTCString()+";path="+path;
        }

}

$(document).ready(function(){
//setCookie();
});


</script>

