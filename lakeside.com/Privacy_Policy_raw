<script type="text/javascript">

	var reg = new RegExp("[0-9][0-9][0-9][0-9][0-9][0-9]");
	$(document).ready(function() {
		// ie8 fix for cart button click
		$(".ie8 .cart-button").on('click', function() {
			location.href = $(this).closest("a").attr("href");
		});
		$("#quickSearch-query").autocomplete("/common/includes/inc_search_type_ahead.jsp", {
			width: 400,
			selectFirst: false,
			cacheLength: 2,
			max: 12,
			extraParams: {environment: 'typeAhead_queries', sort: 'alpha', searchFrom: '',
				Ntt:function(){
					return $("#quickSearch-query").attr('value')+'*';
				}
			},
			minChars: 6,
			formatResult: function(data, value) {
			return null;
			}
		});

		$('#quickSearch-query').keyup( function(evt) {
			if ("LS" != "LS")
			if ((evt.keyCode >= 48 && evt.keyCode <= 90) || (evt.keyCode >= 96 && evt.keyCode <= 105) || (evt.keyCode == 189) || (evt.keyCode == 109)) {
			var typedChars = $(this).val();
			var firstSix = typedChars.substr(0, 6);
			//-- only format input if it's a pin code
			if (typedChars.length >= 6 && reg.test(firstSix)) {
			$(this).val(formatSearchItemNumber($(this)));
			}
			}
		});

		$('#quickSearch-query').result(function(event, data, formatted) {
			if (data) {
			//-- just leave pin code in search box
			var searchTerm = String(data);
			var endOfPin = searchTerm.indexOf(" ");
			$('#quickSearch-query').val(searchTerm.substr(0, endOfPin));
			$('#quickSearch-submit').click();
			}
		});
		$("#quickSearch-query-for-small").autocomplete("/common/includes/inc_search_type_ahead.jsp", {
			width: 400,
			selectFirst: false,
			cacheLength: 2,
			max: 12,
			extraParams: {environment: 'typeAhead_queries', sort: 'alpha', searchFrom: '',
				Ntt:function(){
					return $("#quickSearch-query-for-small").attr('value')+'*';
				}
			},
			minChars: 6,
			formatResult: function(data, value) {
			return null;
			}
		});

		$('#quickSearch-query-for-small').keyup( function(evt) {
			if ("LS" != "LS")
			if ((evt.keyCode >= 48 && evt.keyCode <= 90) || (evt.keyCode >= 96 && evt.keyCode <= 105) || (evt.keyCode == 189) || (evt.keyCode == 109)) {
			var typedChars = $(this).val();
			var firstSix = typedChars.substr(0, 6);
			//-- only format input if it's a pin code
			if (typedChars.length >= 6 && reg.test(firstSix)) {
			$(this).val(formatSearchItemNumber($(this)));
			}
			}
		});

		$('#quickSearch-query-for-small').result(function(event, data, formatted) {
			if (data) {
			//-- just leave pin code in search box
			var searchTerm = String(data);
			var endOfPin = searchTerm.indexOf(" ");
			$('#quickSearch-query-for-small').val(searchTerm.substr(0, endOfPin));
			$('#quickSearch-for-small-submit').click();
			}
		});
	});
	function formatSearchItemNumber(fieldId) {
		var typedChars = fieldId.val();
		typedChars = typedChars.replace(/-/g,"");
		if (typedChars.length >= 10) {
			typedChars = typedChars.substr(0, 6) + "-" + typedChars.substr(6, 4) + "-" + typedChars.substr(10);
		} else {
			if (typedChars.length >= 6) {
				typedChars = typedChars.substr(0, 6) + "-" + typedChars.substr(6);
			}
		}
		if (typedChars.substr(6, 1) == "-") {
			typedChars = typedChars.toUpperCase();
		}
		return typedChars;
	}

	var CONTEXT_ROOT = '/;jsessionid=oXm9IX8SbSkSg-Zq2ExJ95JUzOKvzLMqFivuje9r_P2BXhHSxpia!-1911589501';
		var indexOfSemicolon = CONTEXT_ROOT.indexOf(';');
	if (indexOfSemicolon > 0) {
		CONTEXT_ROOT = CONTEXT_ROOT.substring(0, indexOfSemicolon);
	}

	//-->
</script>

