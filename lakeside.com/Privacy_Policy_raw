		var categoryId = $('#categoryId').val();
		$target = $('#' + categoryId);
		$target.parents('ul.collapse').addClass('in');
		$target.children('a').addClass('selected');
		$target.children('ul').addClass('in');
		if (categoryId !=''){
		//alert(categoryId);
			$target2 = $('#sidebar #' + categoryId);
			$target2.parents('ul.collapse').addClass('in');
			$target2.children('a').addClass('selected');
			//the next 5 lines should be commented out in case they do not want to expand second level category when clicked on
			$target2.children('ul').addClass('in');	
			if ($('#1'+categoryId).hasClass('has-image')){
			$('#1'+categoryId).removeClass("plus");			
				$('#1'+categoryId).addClass("minus");
			}	
			
			var myList = $target2.parents('ul.collapse')
			for (var i = 0; i < myList.length; i++) {
				x=myList[i].id.substring(1);
				$('#1'+x).removeClass("plus");
				$('#1'+x).addClass("minus");			
			}	
		}
	</script>


