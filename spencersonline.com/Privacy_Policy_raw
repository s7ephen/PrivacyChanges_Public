    var searchErrorText = "Please enter a search term and try your search again.";

    function submitSearchForm(searchForm) {
        var searchForm = $("form#headerSearch");
        var searchInputVal = $("#keyword").val();
        if (searchInputVal !== "") {
            searchForm.submit();
