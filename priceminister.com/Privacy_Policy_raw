<script type="text/javascript">
var blurSearchInput = true;
var allCategoriesDivInitialHeight = null;
updateLeftColumnHeight = function(submenuid) {
if (allCategoriesDivInitialHeight !== null && $j('.allCategoriesCtn').height() != allCategoriesDivInitialHeight) {
$j('.allCategoriesCtn').height(allCategoriesDivInitialHeight);
}
// var universeId = $j(domElement).attr('id').replace('universe_', '');
// var correspondingCategory = $j('#categoriesCtn_' + universeId);
var universeId = submenuid;
var correspondingCategory = $j('#' + universeId);
var allCategoriesDiv = $j('.allCategoriesCtn');
var allCategoriesDivOffset = allCategoriesDiv.offset().top;
var allCategoriesDivOuterHeight = allCategoriesDiv.outerHeight(true);
var currentOffset = correspondingCategory.offset().top;
var currentOuterHeight = correspondingCategory.outerHeight(true);
if (currentOffset + currentOuterHeight > allCategoriesDivOffset + allCategoriesDivOuterHeight) {
allCategoriesDivInitialHeight = $j('.allCategoriesCtn').height();
var newAllCategoriesHeight = allCategoriesDiv.height() + currentOffset + currentOuterHeight - allCategoriesDivOffset - allCategoriesDivOuterHeight;
allCategoriesDiv.height(newAllCategoriesHeight);
}
};
// Ce script gère l'ui du formulaire de recherche
var e = $j('#globalSearchCatSlct')
, fakeBtn = $j('#hasSelectValue')
, fldFull = $j('.globalSearchCat')
, searchTextInput = $j('#globalSearch');
// Met à jour le texte de #hasSelectValue avec le nom de la catégorie choisie
writeSearchCat = function() {
var catName = $j(e).children('option:selected').text();
fakeBtn.text(catName);
};
searchBarFocus = function(){
$j('.globalSearchForm').addClass('focus');
$j('.topHeader').addClass('searchFocusIn');
};
searchBarBlur = function(){
$j('.globalSearchForm').removeClass('focus');
$j('.topHeader').removeClass('searchFocusIn');
};
searchBarEvents = function(){
searchTextInput.on('focus', function() {
searchBarFocus();
blurSearchInput = true;
});
searchTextInput.on('blur', function(event) {
if(blurSearchInput){
searchBarBlur();
}
});
/* Tests focus sur le search */
$j('#globalSearchCatSlct').on('change', function() {
searchTextInput.focus();
});
};
// Initialisation du texte du bouton
writeSearchCat();
//$j('#globalSearch').attr("value", "");
// Mise à jour du texte du bouton lors du changement de catégorie
e.on('change keyup', function() {
writeSearchCat();
});
// Ajout de la classe "focus" sur.fld_full au focus du select
e.on('focus', function() {
fldFull.addClass('focus');
});
// Retrait de cette classe lors de la perte du focus
e.on('blur', function() {
fldFull.removeClass('focus');
});
// Focus sur le champ de recherche lors de la sélection d'une catégorie
e.on('change', function() {
// On désactive l'ancien handler autocomplete, et on le recréer avec le bon préfixe
$j('#globalSearch').unautocomplete();
var url = $j(e).children('option:selected').attr("data-url");
if(url != null && url != ""){
completionOptions.prefix = $j(e).children('option:selected').attr('value') + "_";
$j("input#globalSearch").attr('data-sel', PM.Util.categoryParameter(url));
}else{
completionOptions.prefix = "gs_";
$j("input#globalSearch").removeAttr('data-sel');
}
// Le préfixe a été setté, on recrée l'évenement autocomplete
$j('#globalSearch').autocomplete('/completion', completionOptions);
searchTextInput.focus();
searchBarFocus();
searchBarEvents();
});
searchBarEvents();
</script>
