<script type="text/javascript">
	// this script below basically detects if we're running in preview mode and if so then it snifs all hrefs on the page and corrects their urls to point to the preview servlet url
	$( document ).ready(function() {
		var currentUrl = window.location.href;
		if(currentUrl.indexOf('/preview/servlet') > -1){
			function doShopURLsPreview(){
				$('a[href*="/shop/"]').each(function() {
		              var value = $(this).attr('href');
		              $(this).attr('href', value.replace('/shop/', '/webapp/wcs/preview/servlet/'));
		        });
	        }
	        doShopURLsPreview()
	        setTimeout(function(){doShopURLsPreview();},2000);
	        $('form[action*="/shop/"]').each(function() {
	              var value = $(this).attr('action');
	              $(this).attr('action', value.replace('/shop/', '/webapp/wcs/preview/servlet/'));
	        });
	        $('a[href*="/webapp/wcs/stores/servlet/"]').each(function() {
	              var value = $(this).attr('href');
	              $(this).attr('href', value.replace('/webapp/wcs/stores/servlet/', '/webapp/wcs/preview/servlet/'));
	        });
	        
	        function prodURLSPreview(){
				$("a[href*='/product/']").each(function(){
					var thisHREF = $(this).attr("href");
					$(this).attr("href", "/webapp/wcs/preview/servlet/ProductDisplay?catalogId=301&storeId=301&langId=-1&productId=" + $(thisHREF.split("?")[0].split("_")).last()[0]);
				});
			}
			prodURLSPreview();
			$(document).ajaxComplete(function(){prodURLSPreview();});
		        
		} else{
		}
		setTimeout(function(){
		if($(".captchaDiv").length > 0){
			dillardsApp.getCaptchaSecurityCode('I');
		}},300);
	});
	
</script>

