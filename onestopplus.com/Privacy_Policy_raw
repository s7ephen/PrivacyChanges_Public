                                <a href="/Plus-Size-Accessories.aspx?DeptId=12658">Accessories</a>
                                <a href="/Plus-Size-Clearance.aspx?DeptId=17212">Home & Kitchen</a>
                                <a href="/Plus-Size-Clearance.aspx?DeptId=19885">Special Sizes</a>
                                <a href="/Plus-Size-Clearance.aspx?DeptId=20110">Lingerie & Sleepwear</a>
                                <a href="/Plus-Size-Clearance.aspx?DeptId=12623">Swimwear</a>
                                <a href="/Plus-Size-Clearance.aspx?DeptId=24704">Men's Big & Tall</a>
                                <a href="/Plus-Size-Clearance.aspx?DeptId=22367"><span style="color: #ff6633;">Outlet</span></a>
