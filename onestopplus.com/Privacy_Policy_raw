	function emptyIt(e)	{
		try		{	
			if(e)			{
				src = e.srcElement;
				
				if(!src)		{
					var src = e.target;
				}
				
				//alert(src);
				var txtValue = src.value;
				//alert(txtValue);
				var stringObj = new String(txtValue);
			
				if(stringObj.toUpperCase() == "ENTER EMAIL ADDRESS") 	{
					src.value = "";
				}
			}
		}
		catch(ex){}
	}
	
