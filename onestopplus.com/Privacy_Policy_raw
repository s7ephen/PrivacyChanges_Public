<script>

/*
    tabSlideOUt v1.3
    
    By William Paoli: http://wpaoli.building58.com

    To use you must have an image ready to go as your tab
    Make sure to pass in at minimum the path to the image and its dimensions:
    
    example:
    
        $('.slide-out-div').tabSlideOut({
                tabHandle: '.handle',                                   // class of the element that will be your tab -doesnt have to be an anchor
                pathToTabImage: 'images/fb-flyout-mag-promo-tab.jpg',   // relative path to the image for the tab *required*
                tabHeight: '133px',                                     // height of tab image *required*
                tabWidth: '44px',                                       // width of tab image *required*    
        });

    Modified by Marvin Carlos, 2015/06/17, for Full Beauty Brands, tabSlideOUt v1.4

    Changes: 
        a. added option for hiding on scroll slideOutOnScroll()
        b. added vertical tab centering (and recentering on browser resize)
        c. renamed imageHeight to tabHeight, etc.
        d. changed some defaults and settings var property names
        e. bugfix for right positioning on Mac Chrome/Safari (Webkit) browsers
        f. dynamically adjust height slideOut/flyOut for 100% height containers
        g. added isOpen flag (by James McCue) to avoid Omniture retriggering

*/


(function($){
    $.fn.tabSlideOut = function(callerSettings) {
        var settings = $.extend({
            tabHandle: null,
            slideOutCont: null,
            speed: 300, 
            action: 'click',
            tabLocation: 'left',
            topPos: '0px',
            bottomPos: '0px',
            leftPos: '0px',
            fixedPosition: true,
            positioning: 'absolute',
            pathToTabImage: null,
            tabHeight: null,
            tabWidth: null,
            onLoadSlideOut: false   ,
            hideOnScroll: true
        }, callerSettings||{});

        settings.tabHandle = $(settings.tabHandle);
        var obj = this;
        if (settings.fixedPosition === true) {
            settings.positioning = 'fixed';
        } else {
            settings.positioning = 'absolute';
        }
        
        //ie6 doesn't do well with the fixed option
        if (document.all && !window.opera && !window.XMLHttpRequest) {
            settings.positioning = 'absolute';
        }
        
        //get half of tab height to subtract it and get vertical centering properly
        var halfTabHeight = parseInt(settings.tabHeight, 10) / 2; 
        var centerTabPos = ($(window.top).height()) / 2 - halfTabHeight; 

        //workaround for Mac webkite browsers for right positioning that subtracts width of invisible browser scrollbar chrome
        var isChrome = /Chrome/.test(navigator.userAgent) && /Google Inc/.test(navigator.vendor);
        var isSafari = /Safari/.test(navigator.userAgent) && /Apple Computer/.test(navigator.vendor); 
        var isWebKitMobile = navigator.userAgent.match(/(iPod|iPhone|iPad)/) && navigator.userAgent.match(/AppleWebKit/);

        var isOpen = false;

        //set initial tabHandle css
        
        if (settings.pathToTabImage != null) {
            settings.tabHandle.css({
            'background' : 'url('+settings.pathToTabImage+') no-repeat',
            'width' : settings.tabWidth,
            'height': settings.tabHeight
            });
        }
        
        settings.tabHandle.css({ 
            'display': 'block',
            'textIndent' : '-99999px',
            'outline' : 'none',
            'position' : 'absolute'
        });
        
        obj.css({
            'line-height' : '1',
            'position' : settings.positioning
        });

        
        var properties = {
                    containerWidth: parseInt(obj.outerWidth(), 10) + 'px',
                    containerHeight: parseInt(obj.outerHeight(), 10) + 'px',
                    tabWidth: parseInt(settings.tabHandle.outerWidth(), 10) + 'px',
                    tabHeight: parseInt(settings.tabHandle.outerHeight(), 10) + 'px'
                };

        //set calculated css
        if(settings.tabLocation === 'top' || settings.tabLocation === 'bottom') {
            obj.css({'left' : settings.leftPos});
            settings.tabHandle.css({'right' : 0});
        }
        
        if(settings.tabLocation === 'top') {
            obj.css({'top' : '-' + properties.containerHeight});
            settings.tabHandle.css({'bottom' : '-' + properties.tabHeight});
        }

        if(settings.tabLocation === 'bottom') {
            obj.css({'bottom' : '-' + properties.containerHeight, 'position' : 'fixed'});
            settings.tabHandle.css({'top' : '-' + properties.tabHeight});
            
        }
        
        if(settings.tabLocation === 'left' || settings.tabLocation === 'right') {
            obj.css({
                'height' : properties.containerHeight,
                'top' : settings.topPos,
                'bottom' : settings.bottomPos
            });
            
            settings.tabHandle.css({'top' : centerTabPos});
        }
        
        if(settings.tabLocation === 'left') {
            obj.css({ 'left': '-' + properties.containerWidth});
            settings.tabHandle.css({'right' : '-' + properties.tabWidth});
        }

        if(settings.tabLocation === 'right') {
            obj.css({ 'right': '-' + properties.containerWidth});
            settings.tabHandle.css({'left' : '-' + properties.tabWidth});
            
            $('html').css('overflow-x', 'hidden');
        }

        //functions for animation events
        
        settings.tabHandle.click(function(event){
            event.preventDefault();
        });
        
        var slideIn = function() {
            if (isOpen) {
                isOpen = false;
                s_trackMagazineFlyout('Close');
                if (settings.tabLocation === 'top') {
                    obj.animate({top:'-' + properties.containerHeight}, settings.speed).removeClass('open');
                } else if (settings.tabLocation === 'left') {
                   obj.animate({left: '-' + properties.containerWidth}, settings.speed).removeClass('open');
                } else if (settings.tabLocation === 'right') {
                    obj.animate({right: '-' + properties.containerWidth}, settings.speed).removeClass('open');
                } else if (settings.tabLocation === 'bottom') {
                    obj.animate({bottom: '-' + properties.containerHeight}, settings.speed).removeClass('open');
                }    
            }
        };
        
        var slideOut = function() {
            if (!isOpen) {
                isOpen = true;
                s_trackMagazineFlyout('Open');
                if (settings.tabLocation == 'top') {
                    obj.animate({top:'-3px'},  settings.speed).addClass('open');
                } else if (settings.tabLocation == 'left') {
                    obj.animate({left:'-3px'},  settings.speed).addClass('open');
                } else if (settings.tabLocation == 'right') {
                    obj.animate({right:'-3px'},  settings.speed).addClass('open');
                } else if (settings.tabLocation == 'bottom') {
                    obj.animate({bottom:'-3px'},  settings.speed).addClass('open');
                }
            }
        };

        var clickScreenToClose = function() {
            obj.click(function(event){
                event.stopPropagation();
            });
            
            $(document).click(function(){
                slideIn();
            });
        };
        
        var clickAction = function(){
            settings.tabHandle.click(function(event){
                if (obj.hasClass('open')) {
                    slideIn();
                } else {
                    slideOut();
                }
            });
            
            clickScreenToClose();
        };
        
        var hoverAction = function(){
            obj.hover(
                function(){
                    slideOut();
                },
                
                function(){
                    slideIn();
                });
                
                settings.tabHandle.click(function(event){
                    if (obj.hasClass('open')) {
                        slideIn();
                    }
                });
                clickScreenToClose();
                
        };
        
        var slideOutOnLoad = function(){
            slideIn();
            setTimeout(slideOut, 500);
        };

        var slideOutOnScroll = function(){
            $(window).on('scroll', function() {
                //if ( $(window).scrollTop() > 0 ){
                  slideIn();
                //}
            });            
        }
        
        //choose which type of action to bind
        if (settings.action === 'click') {
            clickAction();
        }
        
        if (settings.action === 'hover') {
            hoverAction();
        }

        if (settings.hideOnScroll) {
            slideOutOnScroll();
        }        
        
        if (settings.onLoadSlideOut) {
            slideOutOnLoad();
        };
        
    };

    //re-center content vertically
    $(window).on("resize", function() {
        $(".slide-out").css("height", "100%");
        $(".sl-inner-cont").css("height", "100%");

        $(".sl-tab-handle ").css({
            top : ($(window).height() - $(".sl-tab-handle").outerHeight()) / 2
        })
        //var newcenterTabPos = ($(window.top).height()) / 2 - halfTabHeight; 

    })

})(jQuery);
  

</script>   

<div class="slide-out" style="display:table">
  <div class="sl-inner-cont">
    <a class="sl-tab-handle" href="#">&nbsp;</a>  

    <h4 class="sl-title">
      <em class="sl-title-em">Summer!</em>
      FullBeauty Magazine
    </h4>

    <a class="sl-link" href="http://www.fullbeauty.com/staticpage/contentpage.aspx?pagename=Magazine_June15&contenttype=Magazine&slidenumber=1">Read Now &gt;</a>
    
    <a href="http://www.fullbeauty.com/staticpage/contentpage.aspx?pagename=Magazine_June15&contenttype=Magazine&slidenumber=1" class="sl-feature-image sl-link">
      Read Our<br> New Magazine &gt;
    </a>

    <p class="sl-content-text">
      Fashion Trends
    </p>
    <p class="sl-content-text">
      Beauty Tips
    </p>
    <p class="sl-content-text">
      Recipes
    </p>
    <p class="sl-content-text">
      Celebrities
    </p>
  </div>
</div>   

<script>
$(function(){
  var urlPath = window.location.pathname.toString().toLowerCase();
  var urlQuery = window.location.toString().toLowerCase();
  var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);

  if (isMobile) { 
    $(".slide-out").css("display", "none");
  }

  if (
  !isMobile &&
  !urlPath.indexOf("/shopping_bag/shoppingbag.aspx") == 0 &&
  !urlPath.indexOf("/checkout/netp.aspx") == 0 &&
  !urlPath.indexOf("/checkout/checkout.aspx") == 0 &&
  !urlPath.indexOf("/internationalcheckout/intlshippingaddress.aspx") == 0 &&
  !urlPath.indexOf("/internationalcheckout/intlpaymentprocessinfo.aspx") == 0 &&
  !urlPath.indexOf("/internationalcheckout/intlorderreviewpayment.aspx") == 0 &&
  !urlPath.indexOf("/internationalcheckout/intlshippingmethod.aspx") == 0 &&
  !urlPath.indexOf("/magazine/subscriptionlandingpage.aspx") == 0 &&
        (
      !urlPath.indexOf("/staticpage/contentpage.aspx") == 0 &&
            urlQuery.indexOf("contenttype=magazine") < 0
        ) 
  ) {      
      
    $(".slide-out").tabSlideOut({
    slideOutCont: '.slide-out',                               // class name of container for flyout
    tabHandle: '.sl-tab-handle',                              // class name of visible tab/handle
    tabLocation: 'right',                                     // tab location: right (default), left, top bottom, 
    tabHeight: '176px',                                       // height of tab
    tabWidth: '100px',                                        // width of tab    
    action: 'click',                                          // options: 'click' or 'hover', action to trigger animation
    hideOnScroll: true,                                       // hide on scroll: true (default) or false
    pathToTabImage: 'http://secureimages.plussizetech.com/images/site_images/mastersite/fb-flyout-mag-promo-tab.jpg'
    // path to the image for the tab (can be set using css)
    });

  } else {
    $(".slide-out").css("display", "none");
  }

});

/**Sets whether customer opened or closed magazine fly-out **/

function s_trackMagazineFlyout(action) { 
    var s = s_gi(s_account); 
    s.linkTrackVars ='prop19'; 
    s.prop19 = "US:OSP:Magazine Fly-out " + action;
    s.tl(true, 'o', 'magazine fly-out');
}

</script>

