<script type="text/javascript">
    $(function () {
        $('.btn-cta-sm').each(function (e) {
            var attr = $(this).attr('productnames');
            if (typeof attr !== 'undefined' && attr !== false) {
                $(this).click({ product: attr }, function (e) {
                    e.preventDefault();
                    var linkTo = $(this).attr("href");
                    try {
                        Nina.postqualify(null, 'ProductAddedToCart', e.data.product.split(';')[5], function () {

                            location.href = linkTo;
                        });
                    }
                    catch (err) {
                        location.href = linkTo;
                    }

                });
            }
        });
    });
</script>
   
