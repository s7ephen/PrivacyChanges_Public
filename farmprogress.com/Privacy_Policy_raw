<script type="text/javascript">
$(function() {
	$('#loginButton').click(function() {
	    var jsonArgs = JSON.stringify({
	        "ue" : $(".email-address").val(),
	        "up" : $(".login-password").val(),
	        "rm" : ($(".remember-me").checked)?"Y":"n"
	         });
	    var jsonUrl = "http://farmprogress.com/ws/ua.asmx/ua";
	    var jsonResponse;
	    $.ajax({
	      type: "POST",
	      url: jsonUrl,
	      data: jsonArgs,
	      contentType: "application/json; charset=utf-8",
	      dataType: "json",
	      success: function(jsonResponse) {
	        var responseObj = jQuery.parseJSON(jsonResponse.d);
	        if(responseObj.OK != null ){
	            if(responseObj.OK == "y" ){
	                omnitureEvent( 'event2', 'Login' );
	                window.location.href = "http://farmprogress.com/mySettings.aspx?rnu=2";
	            }else{
	                alert('The email and/or password are incorrect.' );
	            }
	        }else{
	            alert('Login failed. Please try again.' );
	        }
	      }
	    });
	});
	/* END #loginButton click */
	$('#sendPassword').click(function() {
         var jsonArgs = JSON.stringify({
                "ue" : $(".email-address-forgot").val()
                 });
            var jsonUrl = "http://farmprogress.com/ws/ua.asmx/fe";
            var jsonResponse;
            
            $.ajax({
              type: "POST",
              url: jsonUrl,
              data: jsonArgs,
              contentType: "application/json; charset=utf-8",
              dataType: "json",
              success: function(jsonResponse) {
                var response = jsonResponse.d;
                if(response != null ){
                    if(response == true ){
                        alert('The password has been sent to ' + $(".email-address-forgot").val() + '.' );
                    }else{
                        alert('No member with an email address of ' + $(".email-address-forgot").val() + ' was found.' );
                    }
                }else{
                    alert('Password reminder failed. Please try again.' );
                }
              }
            });
      });
      /* END #sendPassword click */
});
</script>
